.fulishe_container {
    width: 100%;
    min-height: 100vh;
}

.fulishe_title {
    display: flex;
    position: relative;
    width: 750rpx;
    height: 192rpx;
}

.fulishe_container .fulishe_title .bg {
    opacity: 1;
    width: 750rpx;
    height: 192rpx;
}

.fulishe_container .fulishe_title .bg image {
    width: 750rpx;
    height: 192rpx!important;
    font-size: 0;
}

.fulishe_container .type_wrap {
    padding: 20rpx 20rpx 0;
    box-sizing: border-box;
    width: 100%;
    display: flex;
}

.fulishe_container .type_wrap .type {
    margin-right: 33rpx;
    width: 108rpx;
    height: 44rpx;
    background: #fff;
    border-radius: 4rpx;
    border: 1rpx solid #aca3a3;
    color: #c7c0bc;
    font-size: 28rpx;
    text-align: center;
    line-height: 44rpx;
}

.fulishe_container .type_wrap .type.active {
    color: #ff5000;
    border: 1rpx solid #584f4f;
}

.fulishe_container .nocoupon {
    padding-top: 172rpx;
    font-size: 30rpx;
    text-align: center;
}

.fulishe_container .nocoupon image {
    width: 264rpx;
    height: 224rpx;
}

.fulishe_container .nocoupon view {
    color: #c7c0bc;
    font-size: 24rpx;
    text-align: center;
}

.fulishe_container .scrollView.fullscreen {
    padding: 20rpx;
}

.fulishe_container .scrollView {
    padding: 15rpx 10rpx 0;
    box-sizing: border-box;
}

.fulishe_container .scrollView .coupon_container {
    padding: 5rpx 10rpx 0;
    box-sizing: border-box;
    width: 100%;
    margin-bottom: 20rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_title {
    position: relative;
    font-size: 22rpx;
    color: #aca3a3;
    margin-bottom: 10rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_title .line {
    position: absolute;
    width: 624rpx;
    height: 1rpx;
    background: #e3e0e0;
    left: 85rpx;
    top: 50%;
    transform: translateY(-50%);
}

.fulishe_container .scrollView .coupon_container .coupon_title .line::after {
    position: absolute;
    left: 30rpx;
    top: -10rpx;
    content: "";
    background: #fff;
    width: 20rpx;
    height: 20rpx;
    border-top: 1rpx solid #e3e0e0;
    border-right: 1rpx solid #e3e0e0;
    transform: rotate(135deg);
}

.fulishe_container .scrollView .coupon_container .coupon_info {
    position: relative;
    display: flex;
    align-items: center;
    background: #fff;
    box-shadow: 0 0 5rpx 0 rgba(0,0,0,.12);
    border-radius: 4rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .alreadyget {
    position: absolute;
    top: 0;
    right: 0;
    width: 81rpx;
    height: 76rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container {
    flex-shrink: 0;
    display: flex;
    flex-flow: column;
    align-items: center;
    justify-content: center;
    width: 184rpx;
    height: 170rpx;
    background: linear-gradient(90deg,#f80,#ff5000);
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container .price_count {
    display: flex;
    align-items: baseline;
    color: #fff;
    font-weight: 700;
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container .price_count .sym {
    font-size: 30rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container .price_count .count {
    font-size: 52rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container .price_count .zhe {
    font-size: 30rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .price_container .price_condition {
    color: #fff;
    font-size: 22rpx;
    font-weight: 400;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container {
    flex-shrink: 0;
    width: 358rpx;
    margin-left: 30rpx;
    display: flex;
    flex-flow: column;
    justify-content: center;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .type_name {
    display: flex;
    align-items: center;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .type_name .type {
    flex-shrink: 0;
    width: 52rpx;
    height: 26rpx;
    background: #584f4f;
    border-radius: 4rpx;
    line-height: 26rpx;
    color: #fff;
    font-size: 20rpx;
    font-weight: 700;
    text-align: center;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .type_name .name {
    margin-left: 4rpx;
    font-size: 24rpx;
    color: #584f4f;
    font-weight: 700;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .use_time {
    font-size: 22rpx;
    color: #aca3a3;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .use_rules {
    margin-top: 22rpx;
    display: flex;
    align-items: center;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .use_rules .rule_title {
    font-size: 20rpx;
    color: #aca3a3;
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .use_rules .xialaselect {
    width: 17rpx;
    height: 10rpx;
    margin-left: 10rpx;
    transition: transform .2s;
    transition: transform .2s,-webkit-transform .2s;
    transform: rotateZ(180deg);
}

.fulishe_container .scrollView .coupon_container .coupon_info .type_container .use_rules .xialaselect.active {
    transform: rotateZ(0deg);
}

.fulishe_container .scrollView .coupon_container .coupon_info .btn {
    padding: 0;
    width: 108rpx;
    height: 42rpx;
    border-radius: 4rpx;
    font-size: 22rpx;
    text-align: center;
    line-height: 42rpx;
}

.fulishe_container .scrollView .coupon_container .coupon_info .getnow {
    color: #fff;
    background: linear-gradient(90deg,#ff854d,#ff5000);
}

.fulishe_container .scrollView .coupon_container .coupon_info .usenow {
    border-radius: 4rpx;
    border: 1rpx solid #ff5000;
    color: #ff5000;
}

.fulishe_container .scrollView .coupon_container .coupon_info .lingguang {
    border-radius: 4rpx;
    border: 1rpx solid #aca3a3;
    color: #aca3a3;
}

.fulishe_container .scrollView .coupon_container .coupon_info button.btn:after {
    border: 0;
}

.fulishe_container .scrollView .coupon_condition {
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    width: 100%;
    background: #fff;
    box-shadow: 0 0 5rpx 0 rgba(0,0,0,.12);
    border-radius: 4rpx;
}

.fulishe_container .scrollView .coupon_condition .condition {
    padding-left: 12rpx;
    position: relative;
}

.fulishe_container .scrollView .coupon_condition .condition .diandian {
    position: absolute;
    top: 10rpx;
    left: -6rpx;
    width: 6rpx;
    height: 6rpx;
    background: #aca3a3;
    border-radius: 6rpx;
}

.fulishe_container .scrollView .coupon_condition .condition .content {
    color: #aca3a3;
    font-size: 20rpx;
}