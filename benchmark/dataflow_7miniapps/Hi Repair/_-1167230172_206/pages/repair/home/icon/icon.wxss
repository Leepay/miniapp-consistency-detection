.iconContainer {
    padding: 0 20rpx 10rpx;
    box-sizing: border-box;
    width: 750rpx;
}

.iconContainer .iconWrap {
    display: flex;
    flex-wrap: wrap;
}

.iconContainer .iconWrap .icon {
    display: flex;
    flex-flow: column;
    align-items: center;
    margin-bottom: 10rpx;
    width: 20%;
    position: relative;
}

.iconContainer .iconWrap .icon .icon-right {
    height: 60rpx;
    position: absolute;
    left: 52%;
    top: -16rpx;
}

.iconContainer .iconWrap .icon .main-image {
    width: 88rpx;
    height: 88rpx;
}

.iconContainer .iconWrap .icon text {
    color: #ffe3b8;
    font-size: 24rpx;
}

.iconContainer .iconWrap .icon .cellKf {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
}

.iconContainer .iconWrap .icon .cellKf1 {
    position: absolute;
    top: 60rpx;
    left: 0;
    opacity: 0;
}

.iconContainer .iconWrap .icon .cellKf2 {
    position: absolute;
    top: 0;
    left: 0;
    width: 100rpx;
    height: 50rpx;
}