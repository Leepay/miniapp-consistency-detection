.devicesContainer {
    padding: 0 20rpx 20rpx;
    box-sizing: border-box;
}

.devicesContainer .devicesWrapper {
    background: #fff;
    border-radius: 10rpx;
    padding: 20rpx 20rpx 0rpx;
    box-sizing: border-box;
}

.devicesContainer .devicesWrapper .titleWrap {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.devicesContainer .devicesWrapper .titleWrap .title {
    color: #2d2a2a;
    font-size: 30rpx;
    font-weight: 700;
}

.devicesContainer .devicesWrapper .titleWrap .changemodel {
    display: flex;
    align-items: center;
    color: #2d2a2a;
    font-size: 22rpx;
    font-weight: 700;
}

.devicesContainer .devicesWrapper .titleWrap .changemodel image {
    width: 20rpx;
    height: 20rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap {
    justify-content: space-between;
    margin-top: 20rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap,.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap {
    display: flex;
    align-items: center;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap .device {
    width: 98rpx;
    height: 98rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap .des {
    margin-left: 20rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap .des .destitle {
    color: #584f4f;
    font-size: 24rpx;
    display: flex;
    align-items: center;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap .des .destitle .subtitle {
    margin-left: 8rpx;
    padding: 4rpx 16rpx;
    background: #ffefe1;
    border-radius: 15px;
    color: #ff7200;
    font-size: 20rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceWrap .des .name {
    color: #2d2a2a;
    font-size: 28rpx;
    margin-top: 10rpx;
    font-weight: 700;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceBtnWrap {
    display: flex;
    align-items: center;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceBtnWrap .deviceBtn {
    height: 36rpx;
    border-radius: 8rpx;
    line-height: 36rpx;
    border: 1px solid rgba(255,114,0,.8);
    color: #ff7200;
    padding: 0rpx 10rpx;
    font-size: 22rpx;
    margin-left: 20rpx;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceBtnWrap .imageBtn {
    font-size: 0;
}

.devicesContainer .devicesWrapper .mydeviceWrap .deviceBtnWrap .imageBtn image {
    width: 300rpx;
}

.devicesContainer .devicesWrapper .camera {
    margin-top: 15rpx;
    margin-right: 32rpx;
    white-space: nowrap;
    color: #aca3a3;
    font-size: 24rpx;
    font-weight: 400;
    position: relative;
    z-index: 2;
    display: inline-block;
}

.devicesContainer .devicesWrapper .camera.active {
    color: #ff7200;
}

.devicesContainer .devicesWrapper .hot-mould {
    width: 355rpx;
    height: 183rpx;
    background: #fff;
    box-shadow: 0rpx 6rpx 18rpx 0rpx rgba(0,0,0,.11);
    border-radius: 8rpx;
    margin-right: 20rpx;
    display: inline-block;
    box-sizing: border-box;
    padding: 20rpx;
    position: relative;
}

.devicesContainer .devicesWrapper .hot-mould .officialimg {
    position: absolute;
    top: 0;
    right: 0;
    width: 100rpx;
    height: 28rpx;
}

.devicesContainer .devicesWrapper .hot-mould .hotWrap {
    display: flex;
    flex-flow: column;
    justify-content: space-between;
    height: 100%;
}

.devicesContainer .devicesWrapper .hot-mould .iphone-wrap {
    width: 100%;
    line-height: 42rpx;
    color: #584f4f;
    font-size: 24rpx;
    font-weight: 400;
    text-align: center;
}

.devicesContainer .devicesWrapper .hot-mould .info-wrap {
    width: 230rpx;
    flex-shrink: 0;
    line-height: 33rpx;
    color: #584f4f;
    font-size: 24rpx;
    font-weight: 400;
    white-space: normal;
}

.devicesContainer .devicesWrapper .hot-mould .goRepair {
    color: #ff7200;
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.devicesContainer .devicesWrapper .hot-mould .goRepair .repair {
    width: 111rpx;
    height: 44rpx;
    background: #ff7200;
    border-radius: 4rpx;
    text-align: center;
    line-height: 44rpx;
    color: #fff;
    font-size: 24rpx;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price {
    display: flex;
    justify-content: space-between;
    align-items: flex-end;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price .priceWrap .underline {
    font-size: 22rpx;
    color: #a4a4a4;
    text-decoration: line-through;
    margin-bottom: -10rpx;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price .priceWrap .nowPrice {
    font-size: 22rpx;
    color: #ff7200;
    display: flex;
    align-items: baseline;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price .priceWrap .nowPrice view {
    font-size: 30rpx;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price .tip {
    width: 135rpx;
    height: 55rpx;
    background: url(https://pic.hiweixiu.com/hiweixiu-mobile/img/zhijiang-bg.png) no-repeat;
    background-size: cover;
    color: #fff;
    font-size: 24rpx;
    text-align: center;
    padding-top: 18rpx;
    padding-left: 18rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 500;
}

.devicesContainer .devicesWrapper .hot-mould .underline_price .tip image {
    width: 9rpx;
    height: 17rpx;
    margin-left: 4rpx;
}

.login-dialog {
    position: fixed;
    top: 44%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
    width: 500rpx;
    height: 280rpx;
    z-index: 16;
    background: #fff;
    border-radius: 16rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
}

.login-dialog .title {
    padding-top: 30rpx;
    font-size: 34rpx;
    font-weight: 600;
    color: #444242;
}

.login-dialog .content {
    font-size: 28rpx;
    color: #696969;
    flex: 1;
    padding-top: 50rpx;
}

.login-dialog .foot {
    display: flex;
    justify-content: space-between;
    width: 100%;
    border-top: 1px solid #eee;
}

.login-dialog .foot .cancel {
    color: #444242;
    font-size: 28rpx;
    border-right: 1px solid #eee;
}

.login-dialog .foot .cancel,.login-dialog .foot button {
    width: 50%;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}

.login-dialog .foot button {
    margin-left: 0;
    margin-right: 0;
    padding-left: 0;
    padding-right: 0;
    box-sizing: border-box;
    text-decoration: none;
    border-radius: 0;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    background-color: transparent;
    color: #ff7200;
    font-size: 30rpx;
}

.login-dialog .foot button::after {
    border: none;
}

.login-dialog .foot .button-hover {
    background-color: transparent;
}

.modal {
    position: fixed;
    z-index: 8;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: rgba(0,0,0,.7);
}

.modal.zindex {
    z-index: 10;
}

.modal.zindex14 {
    z-index: 14;
}

.coupon-dialog {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    border-radius: 20rpx 20rpx 0rpx 0rpx;
    transform: translateY(120%);
    z-index: 12;
}

.coupon-dialog .title {
    padding: 0rpx 30rpx;
    border-bottom: 1px solid #e6e6e6;
    position: relative;
}

.coupon-dialog .title,.coupon-dialog .title .close {
    display: flex;
    align-items: center;
    height: 85rpx;
    justify-content: center;
}

.coupon-dialog .title .close {
    position: absolute;
    right: 20rpx;
    top: 0;
    width: 85rpx;
}

.coupon-dialog .title .close image {
    width: 22rpx;
    height: 22rpx;
}

.coupon-dialog .maxHeight {
    min-height: 500rpx;
    max-height: 950rpx;
    overflow: auto;
    box-sizing: border-box;
}

.coupon-dialog .maxHeight .coupon_container {
    padding: 5rpx 10rpx 0;
    box-sizing: border-box;
    width: 710rpx;
    margin: 0 auto 20rpx;
}

.coupon-dialog .maxHeight .coupon_container:nth-child(1) {
    margin-top: 20rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_title {
    position: relative;
    font-size: 22rpx;
    color: #aca3a3;
    margin-bottom: 10rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_title .line {
    position: absolute;
    width: 624rpx;
    height: 1rpx;
    background: #e3e0e0;
    left: 85rpx;
    top: 50%;
    transform: translateY(-50%);
}

.coupon-dialog .maxHeight .coupon_container .coupon_title .line::after {
    position: absolute;
    left: 30rpx;
    top: -10rpx;
    content: "";
    background: #fff;
    width: 20rpx;
    height: 20rpx;
    border-top: 1rpx solid #e3e0e0;
    border-right: 1rpx solid #e3e0e0;
    transform: rotate(135deg);
}

.coupon-dialog .maxHeight .coupon_container .coupon_info {
    position: relative;
    display: flex;
    align-items: center;
    background: #fff;
    box-shadow: 0 0 5rpx 0 rgba(0,0,0,.12);
    border-radius: 4rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .alreadyget {
    position: absolute;
    top: 0;
    right: 0;
    width: 81rpx;
    height: 76rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container {
    flex-shrink: 0;
    display: flex;
    flex-flow: column;
    align-items: center;
    justify-content: center;
    width: 184rpx;
    height: 135rpx;
    background: linear-gradient(90deg,#f80,#ff5000);
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container .price_count {
    display: flex;
    align-items: baseline;
    color: #fff;
    font-weight: 700;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container .price_count .sym {
    font-size: 30rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container .price_count .count {
    font-size: 52rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container .price_count .zhe {
    font-size: 30rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .price_container .price_condition {
    color: #fff;
    font-size: 22rpx;
    font-weight: 400;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .type_container {
    flex-shrink: 0;
    width: 358rpx;
    margin-left: 30rpx;
    display: flex;
    flex-flow: column;
    justify-content: center;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .type_container .type_name {
    display: flex;
    align-items: center;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .type_container .type_name .type {
    flex-shrink: 0;
    width: 52rpx;
    height: 26rpx;
    background: #584f4f;
    border-radius: 4rpx;
    line-height: 26rpx;
    color: #fff;
    font-size: 20rpx;
    font-weight: 700;
    text-align: center;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .type_container .type_name .name {
    margin-left: 4rpx;
    font-size: 24rpx;
    color: #584f4f;
    font-weight: 700;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .type_container .use_time {
    font-size: 22rpx;
    color: #aca3a3;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .btn {
    padding: 0;
    width: 108rpx;
    height: 42rpx;
    border-radius: 4rpx;
    font-size: 22rpx;
    text-align: center;
    line-height: 42rpx;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .getnow {
    color: #fff;
    background: linear-gradient(90deg,#ff854d,#ff5000);
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .usenow {
    border-radius: 4rpx;
    border: 1rpx solid #ff5000;
    color: #ff5000;
    position: relative;
    z-index: 10;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info .lingguang {
    border-radius: 4rpx;
    border: 1rpx solid #aca3a3;
    color: #aca3a3;
}

.coupon-dialog .maxHeight .coupon_container .coupon_info button.btn:after {
    border: 0;
}