.ljxdbg {
    width: 686rpx;
    min-height: 159rpx;
    border-radius: 10rpx;
    box-shadow: 0 3px 24px 0 hsla(0,0%,63.9%,.2);
    background-size: 100% 6rpx;
    margin: 10rpx auto 0;
}

.add,.ljxdbg {
    display: flex;
    align-items: center;
}

.add {
    background: url(data:image/png;base64,) 626rpx no-repeat;
    background-size: 16rpx 28rpx;
    width: 100%;
}

.content_info {
    max-width: 450rpx;
    padding: 10rpx 0 10rpx 50rpx;
}

.content_info .name_phone {
    display: flex;
    align-items: flex-end;
}

.content_info .name_phone .content_name {
    font-size: 30rpx;
    font-weight: 600;
    color: #000;
}

.content_info .name_phone .content_phone {
    font-size: 24rpx;
    color: #ababab;
    margin-left: 20rpx;
}

.content_address {
    margin-top: 16rpx;
    color: #ababab;
    font-size: 26rpx;
    line-height: 38rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}

.add image {
    width: 64rpx;
    height: 64rpx;
    margin-left: 42rpx;
}

.h45 {
    height: 45rpx;
}

.tianjia {
    height: 85rpx;
    line-height: 85rpx;
    font-weight: 600;
    color: #000;
}

.couponHiB,.priceInfo,.repairInfo,.Tips,.Tips_content,.way {
    width: 686rpx;
    border-radius: 10rpx;
    box-shadow: 0 3px 24px 0 hsla(0,0%,63.9%,.2);
    margin: 32rpx auto 0;
}

.way_center,.way_IMEI,.way_remark,.way_time,.way_way {
    display: flex;
    margin: 0 42rpx;
    align-items: center;
    font-size: 28rpx;
    min-height: 100rpx;
    border-bottom: 1rpx solid #f2f2f2;
}

.way_center_address {
    margin: 0 42rpx;
    padding: 20rpx 0 20rpx 64rpx;
    font-size: 24rpx;
    color: #ababab;
    border-bottom: 1rpx solid #f2f2f2;
    line-height: 40rpx;
}

.way_remark {
    border-bottom: none;
}

.way_center image,.way_IMEI image,.way_remark image,.way_time image,.way_way image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 32rpx;
}

.couponHiB_yh,.Tips_icon,.way_center,.way_time,.way_way {
    background: url(data:image/png;base64,) 100% no-repeat;
    background-size: 16rpx 28rpx;
}

.way_center_choose,.way_time_choose,.way_way_choose {
    color: #ababab;
}

.way_center_choose.centerIschoose,.way_time_choose.timeIschoose,.way_way_choose.wayIschoose {
    color: #000;
}

.couponHiB .couponHiB_dikou,.couponHiB .couponHiB_yh {
    margin: 0 42rpx;
    font-size: 28rpx;
    min-height: 100rpx;
    border-bottom: 1rpx solid #f2f2f2;
    justify-content: space-between;
}

.couponHiB .couponHiB_dikou,.couponHiB .couponHiB_yh,.couponHiB_dikou_l,.couponHiB_dikou_r,.couponHiB_yh_l,.couponHiB_yh_r {
    display: flex;
    align-items: center;
}

.couponHiB_dikou_l image,.couponHiB_yh_l image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 32rpx;
}

.couponHiB_yh_l {
    width: 340rpx;
    color: #ababab;
}

.couponHiB_yh_l .couponHiB_yh_txt.couponIsChoose {
    color: #000;
}

.couponHiB_dikou_r_txt,.couponHiB_yh_r {
    margin-right: 40rpx;
    color: #ababab;
}

.couponHiB_dikou_r switch {
    zoom: .7;
}

.repairInfo {
    display: flex;
    align-items: flex-start;
    font-size: 28rpx;
    min-height: 100rpx;
}

.repairInfo_r {
    padding-right: 42rpx;
    padding-top: 34rpx;
}

.repairInfo image {
    width: 32rpx;
    height: 32rpx;
    margin: 34rpx 32rpx 0 42rpx;
}

.repairInfo .repairInfo_model {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 15rpx;
}

.selectedFault {
    padding: 20rpx 0;
    border-bottom: 1rpx solid #e9e9e9;
}

.selectedFault:last-child {
    border: none;
}

.repairInfo .repairInfo_model .model_name {
    width: 450rpx;
}

.repairInfo .repairInfo_model .model_color {
    color: #ababab;
}

.repairInfo_des,.repairInfo_plan,.repairInfo_price {
    padding-bottom: 10rpx;
    font-size: 24rpx;
}

.repairInfo_des {
    color: #ababab;
}

.repairInfo_price text {
    color: #ff7a00;
}

.priceInfo {
    padding: 16rpx 42rpx;
    box-sizing: border-box;
}

.priceInfo_sum {
    line-height: 48rpx;
    font-size: 26rpx;
    display: flex;
    justify-content: space-between;
}

.priceInfo_sum .priceInfo_sum_num {
    color: #ababab;
}

.Tips {
    box-sizing: border-box;
    padding: 0 42rpx;
}

.Tips_icon {
    display: flex;
    align-items: center;
    min-height: 100rpx;
}

.Tips image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 32rpx;
}

.Tips_btn {
    font-size: 28rpx;
}

.Tips_content {
    font-size: 24rpx;
    color: #ababab;
    padding: 16rpx 42rpx;
    box-sizing: border-box;
    margin-top: 10rpx;
    opacity: 0;
    height: 0;
    overflow: hidden;
    transition: .3s;
}

.Tips_open {
    opacity: 1;
    height: auto;
    padding: 20rpx 42rpx;
    transform: translateY(0);
}

.UserAgreement {
    width: 686rpx;
    font-size: 24rpx;
    color: #000;
    line-height: 86rpx;
    padding-bottom: 130rpx;
}

.UserAgreement text {
    color: #ff7a00;
}

.createFoot {
    width: 686rpx;
    height: 100rpx;
    background: #fff;
    box-shadow: 0 0 10rpx 0 hsla(0,0%,60.4%,.24);
    border-radius: 10rpx;
    margin: 0 auto;
    position: fixed;
    bottom: 30rpx;
    left: 50%;
    transform: translate(-50%,0);
    overflow: hidden;
}

.createFoot,.createFoot .left {
    display: flex;
}

.createFoot .left {
    width: 460rpx;
    align-items: center;
    font-size: 28rpx;
    padding-left: 33rpx;
    box-sizing: border-box;
}

.createFoot .left text {
    font-weight: 600;
    color: #ff7a00;
    margin-left: 23rpx;
}

.createFoot .right {
    flex: 1;
    height: 100%;
    background: #ff7a00;
    color: #fff;
    display: flex;
    font-size: 32rpx;
    align-items: center;
    justify-content: center;
    border-radius: 0;
}

.modalBg {
    height: 100%;
    background: rgba(0,0,0,.3);
    top: 0;
    z-index: 10;
}

.modalBg,.modalContent {
    width: 100%;
    position: fixed;
    left: 0;
}

.modalContent {
    background: #fff;
    bottom: 0;
    z-index: 11;
    transition: transform .3s;
    transition: transform .3s,-webkit-transform .3s;
    border-radius: 10px 10px 0 0;
    padding: 0 16px;
    box-sizing: border-box;
    transform: translateY(100%);
}

.modalTop {
    padding: 20rpx 0;
    width: 100%;
    flex-flow: row;
    border-bottom: 1px solid #e5e5e5;
    font-size: 28rpx;
    color: #000;
}

.modalTop,.type_list_li {
    display: flex;
    justify-content: space-between;
}

.type_list_li {
    padding: 24rpx;
}

.type_list_li_body_title {
    font-size: 28rpx;
    color: #000;
}

.type_list_li_body_des {
    font-size: 24rpx;
    color: #ababab;
}

.type_list_li .circle {
    width: 40rpx;
    height: 40rpx;
    align-self: center;
    border-radius: 40rpx;
}

.type_list_li .circle.active {
    background: url(data:image/png;base64,) 50% no-repeat;
    background-size: 40rpx 40rpx;
}