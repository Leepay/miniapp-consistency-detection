page {
    width: 100%;
    height: 100%;
}

.plr20 {
    padding: 0 20rpx;
    box-sizing: border-box;
}

.selfContainer {
    width: 100%;
    min-height: 100%;
    background: #f9f9f9;
    padding-top: 20rpx;
}

.selfContainer .userInfoWrap {
    position: relative;
    z-index: 1;
    margin: 0 20rpx;
    height: 240rpx;
}

.selfContainer .userInfoWrap .userinfo {
    display: flex;
    align-items: center;
    padding: 30rpx 30rpx 0;
}

.selfContainer .userInfoWrap .userinfo .avatarWrap {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
    overflow: hidden;
}

.selfContainer .userInfoWrap .userinfo .avatarWrap .avatar {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
}

.selfContainer .userInfoWrap .userinfo .please-login {
    color: #2d2a2a;
    margin-left: 20rpx;
    font-weight: 600;
    font-size: 32rpx;
}

.selfContainer .userInfoWrap .msg-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 30rpx 20rpx;
}

.selfContainer .userInfoWrap .msg-wrap .msg-wrap-left {
    display: flex;
}

.selfContainer .userInfoWrap .msg-wrap .msg {
    flex-flow: column;
    margin-right: 50rpx;
}

.selfContainer .userInfoWrap .msg-wrap .msg,.selfContainer .userInfoWrap .msg-wrap .msg-btn {
    display: flex;
    align-items: center;
    position: relative;
}

.selfContainer .userInfoWrap .msg-wrap .msg-btn {
    width: 167rpx;
    height: 48rpx;
    justify-content: center;
    background: #ff7200;
    border-radius: 8rpx;
    font-size: 22rpx;
    font-weight: 500;
    color: #fff;
}

.selfContainer .userInfoWrap .msg-wrap .msg-btn .miaohui {
    width: 46rpx;
    height: 25rpx;
    font-size: 16rpx;
    background: #e92d00;
    border-radius: 13rpx 13rpx 0rpx 13rpx;
    border: 1px solid #fff;
    position: absolute;
    box-sizing: border-box;
    padding: 0 4rpx;
    left: -22rpx;
    top: -8rpx;
}

.selfContainer .userInfoWrap .msg-wrap .msg .num {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
}

.selfContainer .userInfoWrap .msg-wrap .msg .title {
    color: #696969;
    font-size: 22rpx;
}

.selfContainer .userInfoWrap .msg-wrap .msg-btn .erweima {
    width: 30rpx;
    height: 30rpx;
    margin-right: 5rpx;
}

.selfContainer .userInfoWrap .bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 240rpx;
    z-index: -1;
}

.selfContainer .self_model_container {
    padding: 20rpx 20rpx 0;
    background: #f8f8f8;
}

.selfContainer .self_model_container .self_model_container_main {
    border-radius: 8rpx;
    padding: 20rpx;
    background: #fff;
}

.selfContainer .self_model_container .self_model_container_main .titleWrap {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.selfContainer .self_model_container .self_model_container_main .titleWrap .title {
    color: #2d2a2a;
    font-size: 30rpx;
    font-weight: 700;
}

.selfContainer .self_model_container .self_model_container_main .titleWrap .changemodel {
    display: flex;
    align-items: center;
    color: #2d2a2a;
    font-size: 22rpx;
    font-weight: 700;
}

.selfContainer .self_model_container .self_model_container_main .titleWrap .changemodel image {
    width: 20rpx;
    height: 20rpx;
}

.selfContainer .self_model_container .model_name {
    color: #2d2a2a;
    height: 33rpx;
    font-size: 24rpx;
    line-height: 33rpx;
    margin-top: 16rpx;
    display: flex;
    align-items: center;
    font-weight: 600;
}

.selfContainer .self_model_container .model_name .self {
    width: 52rpx;
    height: 28rpx;
    background: #fff2e8;
    border-radius: 6rpx;
    font-size: 18rpx;
    color: #ff7200;
    line-height: 28rpx;
    margin-right: 8rpx;
    text-align: center;
    font-weight: 400;
}

.selfContainer .self_model_container .model_name .tag {
    width: 126rpx;
    height: 28rpx;
    border-radius: 6rpx;
    border: 1rpx solid #ff7200;
    font-size: 18rpx;
    color: #ff7200;
    line-height: 28rpx;
    margin-left: 8rpx;
    text-align: center;
    font-weight: 400;
}

.selfContainer .self_model_container .model_recycle_info_wrap {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    margin-top: 20rpx;
}

.selfContainer .self_model_container .model_recycle_info_wrap image {
    width: 100rpx;
    height: 100rpx;
    margin-right: 26rpx;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap {
    display: flex;
    flex: 1;
    justify-content: space-between;
    align-items: center;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .price {
    color: #ff5000;
    margin-top: -10rpx;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .price .price_title {
    font-size: 24rpx;
    padding: 0 10rpx;
    background: #ffe1c8;
    border-radius: 5px;
    width: 96rpx;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .price .price_max {
    font-size: 22rpx;
    color: #2d2a2a;
    line-height: 30px;
    display: flex;
    align-items: flex-end;
    margin-top: 12rpx;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .price .price_max view {
    margin-left: 6rpx;
    color: #ff7200;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .price .price_max text {
    font-size: 36rpx;
    font-weight: 600;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .btn_wrap {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .btn_wrap .change_model {
    width: 225rpx;
    height: 60rpx;
    background: -webkit-gradient(linear,left top,right top,from(#ffede5),to(#ffe0d2));
    background: linear-gradient(90deg,#ffede5,#ffe0d2);
    border-radius: 4rpx;
    color: #ff5000;
}

.selfContainer .self_model_container .model_recycle_info_wrap .info_wrap .btn_wrap .test_price {
    width: 124rpx;
    height: 48rpx;
    border-radius: 8rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ff7200;
    color: #fff;
    font-size: 24rpx;
    line-height: 48rpx;
}

.selfContainer .banner {
    margin-top: 30rpx;
    height: 136rpx;
}

.selfContainer .listWrap {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin-top: 30rpx;
}

.selfContainer .listWrap .list {
    width: 225rpx;
    height: 166rpx;
    display: flex;
    flex-flow: column;
    align-items: center;
    justify-content: center;
    background: #fff;
    border-radius: 8rpx;
    margin-bottom: 20rpx;
}

.selfContainer .listWrap .list .icon {
    width: 42rpx;
    height: 42rpx;
}

.selfContainer .listWrap .list .listTitle {
    color: #696969;
    font-size: 22rpx;
    margin-top: 10rpx;
}

.selfContainer .officialWrap {
    margin-top: 72rpx;
    text-align: center;
    position: relative;
}

.selfContainer .officialWrap .official {
    width: 417rpx;
    height: 58rpx;
}

.selfContainer .gou {
    position: absolute;
    right: 30rpx;
    bottom: 0;
    width: 108rpx;
    height: 84rpx;
}

.modal {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 21;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.6);
    display: flex;
    justify-content: center;
}

.modal image {
    width: 600rpx;
    height: 452rpx;
    margin-top: 300rpx;
}