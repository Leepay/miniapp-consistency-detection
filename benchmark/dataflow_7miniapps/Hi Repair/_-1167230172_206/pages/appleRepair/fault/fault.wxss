page {
    width: 100%;
    height: 100%;
    background: #f5f5f7;
}

.mt10 {
    margin-top: 10rpx;
}

.mt20 {
    margin-top: 20rpx;
}

.mt40 {
    margin-top: 40rpx;
}

.mr20 {
    margin-right: 20rpx;
}

.p20 {
    padding: 20rpx;
}

.p2040 {
    padding: 40rpx 20rpx 20rpx;
    box-sizing: border-box;
}

.bgf {
    background: #fff;
}

.faultContainer {
    height: 100%;
    display: flex;
}

.faultContainer .scrollview {
    flex: 1;
}

.faultContainer .addBigPadding {
    padding-bottom: 122rpx;
}

.faultContainer .addPadding {
    padding-bottom: 142rpx;
}

.faultContainer .wrap {
    padding: 20rpx;
    box-sizing: border-box;
}

.faultContainer .bannerContainer {
    font-size: 0;
}

.faultContainer .bannerContainer .banner {
    width: 710rpx;
    height: 264rpx;
    font-size: 0;
}

.faultContainer .mouldDetailContainer {
    padding-top: 20rpx;
    transition: all .3s;
}

.faultContainer .mouldDetailContainer .mouldWrap {
    padding: 25rpx 30rpx 80rpx;
    text-align: center;
    border-radius: 10rpx;
    font-size: 0;
}

.faultContainer .mouldDetailContainer .mouldWrap .title {
    color: #8c8c90;
    font-size: 22rpx;
}

.faultContainer .mouldDetailContainer .mouldWrap .mouldNameWrap {
    padding: 12rpx 0;
    display: flex;
    align-items: baseline;
    justify-content: center;
}

.faultContainer .mouldDetailContainer .mouldWrap .mouldNameWrap .mould {
    color: #090909;
    font-size: 46rpx;
    font-weight: 500;
}

.faultContainer .mouldDetailContainer .mouldWrap .mouldNameWrap .change {
    color: #0071e3;
    font-size: 24rpx;
    flex-shrink: 0;
    width: 70rpx;
    text-align: center;
}

.faultContainer .mouldDetailContainer .mouldWrap .desc {
    color: #8c8c90;
    font-size: 24rpx;
}

.faultContainer .mouldDetailContainer .mouldWrap image {
    width: 440rpx;
    height: 440rpx;
    margin-top: 80rpx;
}

.faultContainer .mouldContainer {
    padding-top: 20rpx;
    transition: all .3s;
    backdrop-filter: saturate(180%) blur(20px);
    background-color: hsla(0,0%,100%,.65);
}

.faultContainer .mouldContainer .mouldWrap {
    flex-flow: row;
    align-items: center;
    border-radius: 10rpx;
    padding: 20rpx;
}

.faultContainer .mouldContainer .mouldName,.faultContainer .mouldContainer .mouldWrap {
    display: flex;
    justify-content: space-between;
}

.faultContainer .mouldContainer .mouldName {
    flex-flow: column;
}

.faultContainer .mouldContainer .mouldName .mouldTitle {
    color: #86868b;
    font-size: 24rpx;
}

.faultContainer .mouldContainer .mouldName .mould {
    color: #333;
    font-size: 30rpx;
}

.faultContainer .mouldContainer .mouldChange {
    color: #0071e3;
    font-size: 24rpx;
}

.faultContainer .mouldContainer .mouldChange .imgchange {
    width: 20rpx;
    height: 20rpx;
}

.faultContainer .mouldContainer.fixed {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
    padding: 0 20rpx;
    box-sizing: border-box;
    width: 100%;
}

.faultContainer .wrapperStyle {
    display: flex;
    align-items: center;
    border: 1rpx solid #d6d6d6;
    box-sizing: border-box;
    border-radius: 8rpx;
}

.faultContainer .addBorder {
    border: 1rpx solid #007aff!important;
}

.faultContainer .colorContainer {
    border-radius: 10rpx;
}

.faultContainer .colorContainer .colorTitleWrap {
    color: #090909;
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.faultContainer .colorContainer .colorTitleWrap .colorTitle {
    font-size: 34rpx;
    font-weight: 500;
}

.faultContainer .colorContainer .colorTitleWrap .subTitle {
    color: #007aff;
    font-size: 26rpx;
}

.faultContainer .colorContainer .colorList {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.faultContainer .colorContainer .colorList .colorWrapper {
    width: 325rpx;
    height: 142rpx;
    flex-flow: column;
    justify-content: center;
}

.faultContainer .colorContainer .colorList .colorWrapper .color {
    width: 38rpx;
    height: 38rpx;
    border-radius: 38rpx;
    box-shadow: inset 0 1rpx 1rpx 0rpx rgba(0,0,0,.52);
    overflow: hidden;
}

.faultContainer .colorContainer .colorList .colorWrapper .colorText {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
    margin-top: 10rpx;
}

.faultContainer .colorContainer .colorList .faultWrapper {
    width: 325rpx;
    height: 142rpx;
    padding: 0 40rpx;
    text-align: center;
    flex-flow: column;
    justify-content: center;
    position: relative;
}

.faultContainer .colorContainer .colorList .faultWrapper .biaoqian {
    position: absolute;
    left: -1px;
    top: -2px;
    width: 94rpx;
    height: 32rpx;
}

.faultContainer .colorContainer .colorList .faultWrapper .biaoqian image {
    width: 94rpx;
    height: 32rpx;
}

.faultContainer .colorContainer .colorList .faultWrapper .colorText {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
    position: relative;
}

.faultContainer .colorContainer .colorList .faultWrapper .official {
    width: 115rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 8rpx;
    text-align: center;
    font-size: 23rpx;
    background: #007aff;
    color: #fff;
}

.faultContainer .colorContainer .repairList {
    overflow: hidden;
}

.faultContainer .colorContainer .repairList .repairWrap {
    width: 100%;
    height: 136rpx;
    padding: 0 20rpx;
    flex-flow: row;
    justify-content: space-between;
}

.faultContainer .colorContainer .repairList .repairWrap .repairPrice {
    width: 160rpx;
    flex-shrink: 0;
    color: #666;
    font-size: 30rpx;
    position: relative;
    display: flex;
    flex-flow: column;
    align-items: center;
}

.faultContainer .colorContainer .repairList .repairWrap .official {
    width: 115rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 8rpx;
    text-align: center;
    font-size: 23rpx;
    background: #007aff;
    color: #fff;
}

.faultContainer .colorContainer .repairList .repairWrap .price {
    color: #e30000;
    font-size: 24rpx;
    line-height: 33rpx;
    font-weight: 500;
    width: 100%;
    text-align: right;
}

.faultContainer .colorContainer .repairList .repairWrap .official-price {
    color: #86868b;
    font-size: 22rpx;
    line-height: 30rpx;
    font-weight: 400;
    text-decoration: line-through;
    width: 100%;
    text-align: right;
}

.faultContainer .colorContainer .repairList .repairWrap .repairText {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
}

.faultContainer .colorContainer .repairWayList .repairWayWrap {
    border: 1rpx solid #d6d6d6;
    box-sizing: border-box;
    border-radius: 8rpx;
}

.faultContainer .colorContainer .repairWayList .repairWayWrap .repairWayTitle {
    color: #090909;
    font-size: 30rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #c8c8cc;
    display: flex;
}

.faultContainer .colorContainer .repairWayList .repairWayWrap .repairWayTitle .recommend {
    color: #007aff;
    font-weight: 500;
}

.faultContainer .colorContainer .repairWayList .repairWayWrap .repairWayDes {
    color: #8c8c90;
    font-size: 24rpx;
    padding-top: 20rpx;
}

.reservation {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box;
    background: #fff;
    box-shadow: 0 -5rpx 17rpx 0 rgba(0,0,0,.06);
}

.reservation .button {
    height: 72rpx;
    line-height: 72rpx;
    background: #007aff;
    border: 0;
    color: #fff;
    font-size: 32rpx;
    border-radius: 12rpx;
    text-align: center;
}

.reservation .disabled {
    background: #c8c8c8;
}

.addBtnPadding {
    padding-bottom: 40rpx;
}

.servicebtn {
    z-index: 101;
    transform: translateX(0);
    transition: .3s;
}

.servicebtn.hide {
    transform: translateX(60%);
}

.servicebtn image {
    width: 90rpx;
    height: 90rpx;
}

.kuWrap {
    position: absolute;
    right: 10rpx;
    top: -176rpx;
    padding: 0 24rpx;
    width: 234rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
    z-index: 10;
    display: none;
    background: #fff;
    box-shadow: 0 0 14px 0 rgba(0,0,0,.26);
}

.kuWrap .zskf {
    display: flex;
    align-items: center;
    height: 80rpx;
    width: 100%;
    color: #2d2a2a;
    font-size: 28rpx;
    position: relative;
}

.kuWrap .zskf image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 4rpx;
}

.kuWrap .zskf .miaohui {
    width: 46rpx;
    height: 25rpx;
    text-align: center;
    line-height: 25rpx;
    font-size: 16rpx;
    color: #fff;
    background: #e92d00;
    border-radius: 13rpx;
    position: absolute;
    right: 0;
    top: 4rpx;
}

.kuWrap .cellKf {
    position: absolute;
    top: 10rpx;
    left: 30rpx;
    z-index: 1;
    opacity: 0;
}

.kuWrap .addBottomBorder {
    border-bottom: 1rpx solid #ebebeb;
}

.showKfWrap {
    display: block;
}

.kuWrap::after {
    content: "";
    position: absolute;
    bottom: -20rpx;
    right: 20rpx;
    border: 10rpx solid transparent;
    border-top-color: #fff;
}

.modal {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 21;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.6);
    display: flex;
    justify-content: center;
}

.modal image {
    width: 600rpx;
    height: 452rpx;
    margin-top: 300rpx;
}

.movable-area {
    pointer-events: none;
    z-index: 100;
    width: 100rpx;
    height: 70%;
    position: fixed;
    bottom: 10%;
    right: 0;
}

.movable-view {
    pointer-events: auto;
    height: 100rpx;
    width: 100rpx;
}