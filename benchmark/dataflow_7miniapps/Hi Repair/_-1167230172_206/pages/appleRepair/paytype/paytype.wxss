page {
    width: 100%;
    height: 100%;
}

.commonproblemContainer {
    width: 100%;
    min-height: 100%;
    background: #f5f5f7;
    padding: 20rpx;
    box-sizing: border-box;
}

.commonproblemContainer .commonproblemWrap {
    border-radius: 12rpx;
    background: #fff;
    padding: 36rpx 20rpx 0;
    box-sizing: border-box;
}

.commonproblemContainer .commonproblemWrap .wraptitle {
    color: #111;
    font-size: 30rpx;
    font-weight: 700;
}

.commonproblemContainer .commonproblemWrap .contentWrap {
    padding: 42rpx 0;
}

.commonproblemContainer .commonproblemWrap .contentWrap .title {
    color: #090909;
    font-size: 26rpx;
}

.commonproblemContainer .commonproblemWrap .contentWrap .ans {
    margin-top: 24rpx;
    color: #86868b;
    font-size: 24rpx;
}

.commonproblemContainer .commonproblemWrap .borderbottom {
    border-bottom: 1rpx solid #c8c8cc;
}