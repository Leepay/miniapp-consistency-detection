<view>
    <view class="index-grids" wx:for="{{grids}}" wx:key="index">
        <view bindtap="handleJump" wx:for="{{item}}" wx:for-index="_index" wx:for-item="_item" wx:key="index">
            <image mode="widthFix" src="{{_item.img}}"></image>
            <text>{{_item.txt}}</text>
        </view>
    </view>
    <view class="button-sp-area fixed-btn">
        <button bindtap="handleJump" class="weui-btn">创建报名</button>
    </view>
</view>
