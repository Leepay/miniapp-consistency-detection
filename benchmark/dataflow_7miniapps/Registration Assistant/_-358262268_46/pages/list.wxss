page {
    font-size: 16px;
    padding-bottom: 140rpx;
}

.withdraw {
    margin-bottom: 20rpx;
    padding: 30rpx 20rpx;
    background-color: #fff;
}

.withdraw .jump {
    padding: 5rpx 50rpx;
    font-size: 14px;
    color: #1aad19;
    border: 1px solid #1aad19;
    border-radius: 10rpx;
}

.navbar .weui-navbar {
    border: 0;
    background-color: #fff;
}

.navbar .page,.navbar .page__bd {
    height: 100%;
}

.navbar .page__bd {
    padding-bottom: 0;
}

.navbar .weui-tab__content {
    padding-top: 60px;
    text-align: center;
}

.navbar .weui-bar__item_on .weui-navbar__title {
    color: #ff5001;
}

.weui-tab__panel {
    padding-top: 46px;
}

.weui-navbar__slider {
    width: 100px;
    background-color: #ff5001;
}

.weui-navbar__item {
    padding: 20rpx 0;
}

.weui-panel__hd {
    font-size: 16px;
    color: #000;
}

.weui-panel__hd .weui-cell__bd {
    font-weight: 700;
}

.weui-media-box__desc {
    padding: 7rpx 0;
    color: #969696;
}

.weui-panel {
    margin-top: 6rpx;
}

.weui-navbar {
    z-index: 10;
}

.box_wrapper {
    display: flex;
    align-items: center;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    padding: 0 10px;
}

.watch {
    padding: 0 20rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 14px;
    color: #ff5001;
    border: 1px solid #ff5001;
    border-radius: 15rpx;
    background-color: inherit;
}

.weui-footer {
    margin: 20rpx 0;
}