.shareWrapper {
    width: 100%;
    padding: 50rpx 0 20rpx;
    background-color: #f8f8f8;
}

.shareWrapper button,.shareWrapper navigator {
    height: 200rpx;
    line-height: 50rpx;
}

.shareWrapper .suc {
    margin-right: 20rpx;
    font-size: 34px;
    color: #1aad19;
}

.shareWrapper button::after {
    border: 0;
}

.shareWrapper .weui-flex__item {
    flex-direction: column;
    font-size: 14px;
    font-weight: 700;
}

.shareWrapper .share_title,.shareWrapper .weui-flex__item {
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
}

.shareWrapper .share_title {
    margin-bottom: 50rpx;
    font-size: 22px;
}

.shareWrapper .share_title image {
    width: 90rpx;
    height: 90rpx;
}

.shareWrapper image {
    width: 100rpx;
    height: 100rpx;
    margin-bottom: 20rpx;
}

./components/cMask.wxssrpx.weui-form-preview {
    overflow: hidden;
    width: 90%;
    margin: 0 auto;
    border-radius: 20rpx;
}

.weui-form-preview image {
    width: 50px;
    height: 50px;
    border-radius: 100%;
}

.weui-form-preview .weui-cell__bd {
    padding: 20rpx;
    font-weight: 700;
    text-align: left;
}

.weui-form-preview .close {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    font-size: 34px;
}

page {
    padding-bottom: 100rpx;
}

.banner {
    position: relative;
    height: 500rpx;
}

.banner image {
    width: 100%;
    height: 500rpx;
}

.banner .title {
    z-index: 1;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 24rpx 30rpx;
    font-size: 16px;
    font-weight: 700;
    color: #fff;
    background-color: rgba(0,0,0,.6);
}

.map_icon {
    width: 20px;
    height: 20px;
    margin-right: 5px;
    vertical-align: middle;
}

.red {
    color: red;
}

.weui-media-box image {
    width: 100%;
}

.weui-cell {
    padding-top: 24rpx;
    padding-bottom: 24rpx;
    font-size: 16px;
}

.weui-media-detail,.weui-panel__hd {
    font-size: 16px;
    color: #000;
}

.weui-media-detail {
    margin-bottom: 40rpx;
    line-height: 24px;
}

.scroll-view_H {
    width: auto;
    padding: 0 20rpx;
    white-space: nowrap;
}

.scroll-view_H view {
    position: relative;
    display: inline-block;
    width: 100rpx;
    margin: 10rpx 0;
    padding: 10rpx 0;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.scroll-view_H image {
    display: block;
    width: 80rpx;
    height: 80rpx;
    border: 1px solid #ccc;
    border-radius: 100%;
}

.tabbar_bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    background-color: #fff;
}

.tabbar_bottom .weui-flex__item {
    padding: 10rpx 0 0;
    font-size: 12px;
    line-height: 40rpx;
    color: #666;
    border-right: 1px solid #ccc;
}

.tabbar_bottom .weui-flex__item image {
    width: 40rpx;
    height: 40rpx;
}

.tabbar_bottom .submit {
    display: flex;
    flex: 2;
    align-items: center;
    justify-content: center;
    font-size: 16px;
    background-color: #ff5e00;
    border-right: 0;
    color: #fff;
}

.tabbar_bottom .disabled {
    background-color: #ccc;
}

.box_wrapper {
    display: flex;
    align-items: center;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    padding: 0 10px;
}

.topImg image {
    display: block;
    width: 120rpx;
    height: 120rpx;
    border: 1px solid #ccc;
    border-radius: 100%;
}

.watch {
    padding: 0 60rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 14px;
    color: #ff5001;
    border: 1px solid #ff5001;
    border-radius: 15rpx;
    background-color: inherit;
}

.ad {
    text-align: center;
}

.ad .line {
    height: 1px;
    margin: 20px 30rpx;
    border-top: 1px dashed #d9d9d9;
}

.ad image {
    width: 710rpx;
    height: 270rpx;
}