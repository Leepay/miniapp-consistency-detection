.mask-wrapper {
    z-index: 10;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    overflow: hidden;
}

.mask-wrapper .mask {
    z-index: -1;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
}

.mask-wrapper .mask-content {
    z-index: 1;
    position: absolute;
    width: 100%;
    display: flex;
}

.mask-wrapper .content-bottom {
    bottom: 0;
    align-items: flex-end;
    transform: translateY(0);
}

.mask-wrapper .content-center {
    top: 50%;
    align-items: center;
    transform: translateY(-50%);
}

.cDropdown {
    width: 100%;
    color: #333;
}

.cDropdown .cDropdown-title {
    padding: 30rpx 0;
    font-size: 18px;
    font-weight: 700;
    color: #666;
}

.cDropdown .cDropdown-list {
    max-height: 400rpx;
}

.cDropdown button,.cDropdown navigator,.cDropdown view {
    height: 120rpx;
    font-size: 18px;
    text-align: center;
    line-height: 120rpx;
    border-top: 1px solid #d5d5d6;
    background-color: #fff;
    border-radius: 0;
}

./components/cMask.wxssrpx.cDialog {
    width: 80%;
    margin: 0 auto;
    background-color: #fff;
    border-radius: 10rpx;
}

.cDialog .cDialog-title {
    height: auto;
    padding: 40rpx 0 30rpx;
    line-height: normal;
    text-align: center;
    color: #000;
}

.cDialog .cDialog-content {
    padding-bottom: 50rpx;
    font-size: 32rpx;
    text-align: center;
    color: #999;
    border-bottom: 1rpx solid #d5d5d6;
}

.cDialog .cDialog-btn {
    display: flex;
    font-size: 38rpx;
    text-align: center;
}

.cDialog .cDialog-btn button,.cDialog .cDialog-btn navigator {
    flex: 1;
    height: 100rpx;
    padding: 0;
    font-size: 16px;
    line-height: 100rpx;
    color: #353535;
}

.cDialog .cDialog-btn button::after,.cDialog .cDialog-btn navigator::after {
    border: 0;
}

.cDialog .column {
    flex-direction: column;
}

.cDialog .column button,.cDialog .column navigator {
    width: 100%;
    border-bottom: 1rpx solid #d5d5d6;
}

