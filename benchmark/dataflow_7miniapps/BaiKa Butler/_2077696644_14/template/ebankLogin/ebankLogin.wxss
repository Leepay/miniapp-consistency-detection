.ebank-body {
    margin-top: 60rpx;
}

.ebank-item {
    margin-left: 62rpx;
    margin-right: 62rpx;
    height: 100rpx;
    background: #fff;
    border-radius: 50rpx;
    margin-top: 35rpx;
    position: relative;
    display: flex;
    align-items: center;
    font-size: 26rpx;
}

.ebank-item-input {
    flex: 1;
    margin-left: 30rpx;
    display: inline-block;
    line-height: 100rpx;
}

.img-item {
    display: inline-block;
    width: 35rpx;
    margin-left: 25rpx;
    height: 35rpx;
}

.ebank-bottom {
    position: absolute;
    bottom: 60rpx;
    left: 62rpx;
    right: 62rpx;
}

.btn-main {
    margin-top: 50rpx;
}

.agree-body {
    height: 50rpx;
    line-height: 50rpx;
    display: flex;
    align-items: center;
    margin-left: 62rpx;
    margin-top: 30rpx;
}