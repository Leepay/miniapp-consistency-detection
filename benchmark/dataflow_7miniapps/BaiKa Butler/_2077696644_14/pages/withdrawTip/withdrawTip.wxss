.head {
    margin-top: 40rpx;
    font-size: 30rpx;
    margin-bottom: 30rpx;
}

.head,.item {
    margin-left: 30rpx;
}

.item {
    font-size: 26rpx;
    margin-top: 20rpx;
    line-height: 40rpx;
    margin-right: 30rpx;
}

.item-manager {
    display: inline;
    border: none;
    font-size: 28rpx;
    line-height: 40rpx;
    background: transparent;
}

.item-manager::after {
    border: none;
}