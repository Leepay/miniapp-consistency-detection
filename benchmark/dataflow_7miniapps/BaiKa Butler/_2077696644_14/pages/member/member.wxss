.background-img {
    height: 395rpx;
    width: 100%;
}

.body {
    position: absolute;
    top: 50rpx;
    width: 728rpx;
    left: 10rpx;
    right: 10rpx;
}

.body,.member-progress-position {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.member-progress-position {
    height: 435rpx;
    width: 100%;
    position: relative;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
    flex-wrap: wrap;
    z-index: 20;
}

.member-head-img {
    display: block;
    width: 165rpx;
    height: 165rpx;
    border-radius: 50%;
    margin-top: 68rpx;
}

.member-grade-img {
    width: 100%;
    height: 50rpx;
    margin-top: 60rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.member-grade-progress-img {
    display: inline-block;
    height: 15rpx;
    width: 508rpx;
}

.member-grade-head-img {
    display: flex;
    align-items: center;
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
}

.item-member-grade-head-body {
    display: inline-flex;
    flex: 1;
    align-items: center;
    justify-content: center;
}

.item-member-grade-head-img {
    width: 50rpx;
    height: 50rpx;
}

.member-head {
    line-height: 28rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
}

.member-grade-text {
    display: flex;
    align-items: center;
    height: 60rpx;
    line-height: 60rpx;
    width: 100%;
    font-size: 23rpx;
}

.item-member-grade {
    flex: 1;
    text-align: center;
}

.item-member-normal-color {
    color: #787878;
}

.item-member-check-color {
    color: #000;
}

.my-rights {
    width: 700rpx;
    background-color: #fff;
    margin-top: 30rpx;
    border-radius: 5rpx;
    padding-bottom: 40rpx;
    padding-top: 30rpx;
}

.rights-title {
    display: flex;
    align-items: center;
    height: 50rpx;
    line-height: 50rpx;
}

.img-title-icon {
    display: inline-block;
    width: 30rpx;
    margin-left: 40rpx;
    margin-right: 10rpx;
    height: 26rpx;
}

.rights-body {
    display: flex;
}

.item-rights {
    display: -webkit-inline-flex;
    flex-direction: column;
    align-items: center;
    flex: 1;
    font-size: 26rpx;
}

.item-rights-img {
    margin-top: 30rpx;
    width: 100rpx;
    height: 100rpx;
}

.item-rights-img,.item-rights-img-no {
    display: inline-block;
    margin-bottom: 10rpx;
}

.item-rights-img-no {
    margin-top: 23rpx;
    margin-left: 35rpx;
    width: 131rpx;
    height: 107rpx;
}

.rules {
    margin-top: 15rpx;
    font-size: 26rpx;
}

.rules-img {
    width: 700rpx;
}

.bottom {
    border-top: 1rpx solid #efefef;
    display: flex;
    position: fixed;
    bottom: 0;
    height: 80rpx;
    line-height: 80rpx;
    width: 100%;
    z-index: 30;
    background: #fff;
}

.bottom-tip {
    flex: 1;
    display: block;
    font-size: 26rpx;
    margin-left: 63rpx;
}

.btn-pay {
    width: 220rpx;
    color: #fff;
    height: 100%;
    font-size: 30rpx;
    border-radius: 0rpx;
}

.btn-pay::after {
    border: none;
}

.good-info {
    width: 700rpx;
    background: #fff;
    padding-bottom: 20rpx;
    padding-top: 20rpx;
}

.radio-body {
    margin-top: 40rpx;
}

.item-price {
    margin-left: 28rpx;
    width: 640rpx;
    margin-right: 28rpx;
    margin-bottom: 15rpx;
    display: flex;
    flex-direction: column;
}

.item-price-normal {
    background-color: #f0f0f0;
    height: 111rpx;
    width: 100%;
}

.item-price-check {
    height: 229rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
    flex-direction: column;
}

.item-price-check,.item-price-head {
    width: 100%;
    display: flex;
}

.item-price-head {
    height: 111rpx;
    align-items: center;
}

.item-price-radio {
    margin-left: 28rpx;
}

radio .wx-radio-input {
    height: 37rpx;
    width: 37rpx;
    border-radius: 50%;
    border: 2rpx solid #999;
    background: transparent;
}

radio .wx-radio-input.wx-radio-input-checked::before {
    border-radius: 50%;
    width: 37rpx;
    height: 37rpx;
    line-height: 37rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    transform: translate(-50%,-50%) scale(1);
}

.ic-item-price {
    width: 50rpx;
    height: 50rpx;
    margin-left: 26rpx;
}

.item-good-name {
    font-size: 30rpx;
    color: #323232;
    margin-left: 28rpx;
    flex: 1;
}

.item-price-num {
    font-size: 34rpx;
    color: #363636;
}

.item-btn-detail {
    width: 71rpx;
    font-size: 20rpx;
    line-height: 26rpx;
    text-align: center;
    margin-right: 15rpx;
    margin-left: 12rpx;
    justify-content: center;
    height: 71rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

.item-btn-detail,.item-price-detail {
    display: flex;
    align-items: center;
}

.item-price-detail {
    height: 118rpx;
    width: 100%;
}

.item-detail-part {
    display: block;
    flex: 1;
    font-size: 26rpx;
    text-align: center;
    line-height: 36rpx;
    color: #a25f3d;
}

.tips {
    color: #777;
    font-size: 24rpx;
    margin: 30rpx 30rpx 150rpx;
    text-indent: 2em;
}

.rule-image {
    width: 700rpx;
    margin: 25rpx;
}

.wechatBox {
    margin: 25rpx;
    width: 660rpx;
    display: flex;
    justify-content: space-between;
    font-size: 24rpx;
    line-height: 40rpx;
    background: #fff;
    position: relative;
    padding: 20rpx;
}

.wechatBox text {
    color: #0020e4;
    border-bottom: 1rpx solid #ddd;
}