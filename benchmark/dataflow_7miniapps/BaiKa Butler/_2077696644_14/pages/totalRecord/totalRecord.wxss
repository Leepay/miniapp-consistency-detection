.head {
    position: absolute;
    left: 27rpx;
    top: 23rpx;
    width: 695rpx;
    height: 284rpx;
    flex-direction: column;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%;
}

.head,.head-title {
    display: flex;
}

.head-title {
    align-items: center;
    height: 106rpx;
    width: 100%;
    color: #fff;
}

.head-title-tip {
    margin-left: 55rpx;
    font-size: 30rpx;
}

.head-title-num {
    margin-left: 47rpx;
    font-size: 32rpx;
}

.head-body {
    display: flex;
    flex: 1;
}

.head-body,.item-head-body {
    align-items: center;
}

.item-head-body {
    height: 100%;
    display: inline-flex;
    flex-direction: column;
    flex: 1;
}

.item-head-line {
    display: inline-block;
    width: 1rpx;
    height: 179rpx;
}

.item-head-body-ic {
    width: 30rpx;
    height: 30rpx;
    margin-top: 24rpx;
}

.head-body-title {
    font-size: 25rpx;
    line-height: 25rpx;
    height: 25rpx;
    color: #3a3a3a;
}

.head-body-number,.head-body-title {
    letter-spacing: 2rpx;
    display: block;
    margin-top: 20rpx;
}

.head-body-number {
    font-size: 27rpx;
    line-height: 27rpx;
    color: #000;
    height: 27rpx;
}

.body {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    top: 310rpx;
}

.scroll-list {
    height: 100%;
    width: 100%;
}

.item {
    height: 211rpx;
    background-image: url("data:image/png;base64,");
    margin-bottom: 6rpx;
    width: 688rpx;
    margin-left: 31rpx;
    background-size: 100% 100%;
}

.item-bank-info {
    width: 100%;
    height: 74rpx;
    display: flex;
    align-items: center;
}

.item-ic-bank {
    width: 34rpx;
    height: 34rpx;
    margin-left: 34rpx;
    border-radius: 50%;
}

.item-bank-name {
    font-size: 26rpx;
    margin-left: 21rpx;
    color: #000;
    flex: 1;
}

.btn-detail {
    margin-right: 34rpx;
    font-size: 20rpx;
    color: #3b3b3b;
    height: 30rpx;
    line-height: 30rpx;
    display: block;
    width: 94rpx;
    border: 1rpx solid #3b3b3b;
    text-align: center;
}

.item-body {
    height: 119rpx;
    width: 100%;
}

.item-body,.item-body-part {
    display: flex;
}

.item-body-part {
    flex: 1;
    flex-direction: column;
}

.item-body-title {
    margin-left: 61rpx;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.item-body-num {
    margin-top: 5rpx;
    margin-left: 61rpx;
    font-size: 26rpx;
}