.ebank-body {
    margin-top: 60rpx;
}

.ebank-item {
    margin-left: 62rpx;
    margin-right: 62rpx;
    height: 100rpx;
    background: #fff;
    border-radius: 50rpx;
    margin-top: 35rpx;
    position: relative;
    display: flex;
    align-items: center;
    font-size: 26rpx;
}

.ebank-item-input {
    flex: 1;
    margin-left: 30rpx;
    display: inline-block;
    line-height: 100rpx;
}

.img-item {
    display: inline-block;
    width: 35rpx;
    margin-left: 25rpx;
    height: 35rpx;
}

.ebank-bottom {
    position: absolute;
    bottom: 60rpx;
    left: 62rpx;
    right: 62rpx;
}

.btn-main {
    margin-top: 50rpx;
}

.agree-body {
    height: 50rpx;
    line-height: 50rpx;
    display: flex;
    align-items: center;
    margin-left: 62rpx;
    margin-top: 30rpx;
}

.body {
    margin-top: 60rpx;
    margin-left: 39rpx;
    margin-right: 39rpx;
    background: #fff;
    flex-direction: column;
    border: 1rpx solid #d0d0d0;
}

.body,.input-bill-head {
    display: flex;
    align-items: center;
}

.input-bill-head {
    background: #d9d9d9;
    margin-top: 36rpx;
    height: 187rpx;
    width: 636rpx;
    border-radius: 15rpx;
}

.img-bank-bg {
    height: 100rpx;
    width: 100rpx;
    background: #fff;
    border-radius: 50%;
    margin-left: 43rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.bank-icon {
    width: 91rpx;
    height: 91rpx;
    border-radius: 50%;
}

.bank-info {
    font-size: 30rpx;
    line-height: 40rpx;
    margin-left: 20rpx;
}

.bank-info,.input-bill-body {
    display: flex;
    flex-direction: column;
}

.input-bill-body {
    align-items: center;
    margin-top: 65rpx;
}

.input-bill-sum {
    border-bottom: 1rpx solid #999;
    width: 300rpx;
    height: 100rpx;
    font-size: 50rpx;
    line-height: 100rpx;
    text-align: center;
}

.head-balance {
    flex: 1;
    margin-right: 40rpx;
    display: flex;
    flex-direction: column;
    text-align: right;
}

.banance-num {
    font-size: 38rpx;
}

.banance-tip {
    font-size: 25rpx;
}

.bill-tip {
    width: 672rpx;
    margin-left: 39rpx;
    background: #fff;
    border: 1rpx solid #d0d0d0;
    flex-direction: column;
    font-size: 25rpx;
}

.bill-tip,.member-name {
    margin-top: 30rpx;
    display: flex;
    align-items: center;
}

.member-name {
    width: 100%;
}

.item-member-title {
    width: 220rpx;
    text-align: right;
}

.item-member {
    text-align: center;
    flex: 1;
}

.member-line {
    margin-top: 40rpx;
    background: #d0d0d0;
    width: 100%;
    height: 1rpx;
}

.repay-tip {
    color: #7a7a7a;
    margin-top: 30rpx;
}

.btn-go {
    margin-top: 26rpx;
    display: block;
    width: 155rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #e64e80;
    border-radius: 25rpx;
    background: #fbecf1;
    border: 1px solid #e64e80;
    min-width: 155rpx;
    min-height: 50rpx;
    font-size: 25rpx;
}

.btn-go:active {
    background: rgba(251,236,241,.6);
}

.btn-go::after {
    border: none;
}

.btn-main {
    margin-top: 50rpx;
    width: 570rpx;
    margin-bottom: 40rpx;
}

.head-blank {
    height: 30rpx;
}

.menu {
    background: #fff;
    height: 60rpx;
    width: 550rpx;
    border-radius: 30rpx;
    margin-left: 100rpx;
}

.item-menu {
    color: #323232;
    font-size: 24rpx;
}

.item-menu,.item-menu-check {
    display: inline-block;
    height: 60rpx;
    width: 275rpx;
    text-align: center;
    line-height: 60rpx;
}

.item-menu-check {
    border-radius: 30rpx;
    color: #fff;
}

.channel-rule {
    margin: 39rpx;
    padding: 39rpx;
    background: #fff;
    border: 1rpx solid #d0d0d0;
}

.channel-rule button {
    margin-top: 40rpx;
    width: 261rpx;
    height: 59rpx;
    line-height: 59rpx;
    font-size: 26rpx;
    color: #e64e80;
    background: #fbecf1;
    border: 1rpx solid #e64e80;
    border-radius: 15rpx;
}

.rule-box {
    justify-content: space-around;
}

.rule-box,.rule-box view {
    display: flex;
}

.rule-box view {
    align-items: center;
    flex-direction: column;
    justify-content: space-between;
    color: #323232;
    font-size: 25rpx;
    height: 150rpx;
}

.rule-box view:last-child text {
    color: #e64e80;
}