.head {
    width: 100%;
}

.head-status {
    height: 80rpx;
    width: 100%;
    position: relative;
    line-height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.btn-head-contact {
    position: absolute;
    background: transparent;
    width: 100%;
    height: 100%;
}

.btn-head-contact::after {
    border: none;
}

.head-error-status {
    background: #ffdddc;
}

.head-error-status-tip {
    color: #ba0101;
}

.img-head-status {
    width: 25rpx;
    height: 25rpx;
}

.head-status-tip {
    margin-left: 30rpx;
    font-size: 24rpx;
}

.head-num {
    background: #fff;
    height: 171rpx;
}

.head-num,.head-num .part {
    display: flex;
}

.head-num .part {
    flex: 1;
    flex-direction: column;
    height: 100%;
    align-items: center;
    justify-content: center;
}

.normal-num {
    color: #000;
}

.blue-num {
    color: #0f57b0;
}

.head-num .part .num {
    font-size: 45rpx;
    line-height: 45rpx;
}

.head-num .part .tip {
    margin-top: 20rpx;
    font-size: 25rpx;
    line-height: 23rpx;
    color: #505050;
}

.head-tip {
    background: #2b65ab;
    padding: 10rpx 57rpx;
}

.head-tip .item-tip {
    font-size: 27rpx;
    line-height: 40rpx;
    display: block;
    color: #fff;
}

.item {
    width: 695rpx;
    height: 233rpx;
    margin-left: 27rpx;
    margin-top: 20rpx;
    position: relative;
    background-size: 100% 100%;
}

.item,.item-bank-info {
    display: flex;
}

.item-bank-info {
    width: 127rpx;
    height: 100%;
    flex-direction: column;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
}

.item-bank-ic {
    width: 42rpx;
    height: 42rpx;
    border-radius: 50%;
    margin-top: 63rpx;
}

.item-bank-num {
    margin-top: 26rpx;
}

.item-bank,.item-bank-num {
    height: 26rpx;
    line-height: 26rpx;
    display: block;
}

.item-bank {
    margin-top: 12rpx;
}

.item-body {
    flex: 1;
    display: inline-flex;
    flex-direction: column;
    height: 100%;
}

.item-body-a {
    height: 130rpx;
}

.item-body-a,.item-body-a-part {
    display: flex;
}

.item-body-a-part {
    flex: 1;
    flex-direction: column;
    height: 100%;
    align-items: center;
}

.item-body-a-title {
    font-size: 24rpx;
    line-height: 24rpx;
    margin-top: 40rpx;
    color: #484848;
}

.item-body-a-num {
    font-size: 30rpx;
    line-height: 24rpx;
    margin-top: 17rpx;
}

.item-body-b {
    height: 102rpx;
}

.item-body-b,.item-body-b-part {
    display: flex;
}

.item-body-b-part {
    flex: 1;
    height: 100%;
    align-items: center;
    font-size: 24rpx;
}

.item-body-b-title {
    margin-left: 46rpx;
    color: #484848;
}

.item-body-b-money {
    margin-left: 28rpx;
    color: #000;
    font-size: 26rpx;
}

.item-body-b-day-num {
    font-size: 42rpx;
    color: #ffb700;
}

.item-body-b-day {
    color: #484848;
    font-size: 18rpx;
    margin-left: 10rpx;
}

.item-btn-more {
    position: absolute;
    top: 0;
    right: 0;
    font-size: 20rpx;
    height: 32rpx;
    width: 98rpx;
    text-align: center;
    display: block;
    color: #fff;
    background: #c1c1c1;
    line-height: 32rpx;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

