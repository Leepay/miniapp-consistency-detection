.head {
    background-image: url("data:image/png;base64,");
    height: 262rpx;
    width: 694rpx;
    margin-left: 28rpx;
    margin-top: 22rpx;
    background-size: 100% 100%;
    flex-direction: column;
}

.head,.head-bank {
    display: flex;
}

.head-bank {
    height: 74rpx;
    width: 100%;
    align-items: center;
}

.ic-head-bank {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    margin-left: 38rpx;
}

.head-bank-info {
    font-size: 26rpx;
    color: #fff;
    margin-left: 25rpx;
}

.head-item {
    height: 85rpx;
}

.head-item,.head-item-part {
    display: flex;
    align-items: center;
}

.head-item-part {
    flex: 1;
    font-size: 30rpx;
    color: #343434;
    line-height: 30rpx;
}

.head-item-title {
    margin-left: 40rpx;
}

.head-item-sum {
    margin-left: 5rpx;
}

.item-menu {
    display: flex;
    align-items: center;
    height: 60rpx;
}

.item-menu-title {
    font-size: 27rpx;
    margin-left: 15rpx;
    color: #343434;
}

.ic-item-menu {
    margin-left: 10rpx;
    margin-right: 10rpx;
    width: 12rpx;
    height: 8rpx;
}

.item-menu-picker {
    margin-top: 10rpx;
    margin-left: 35rpx;
    display: flex;
    width: 150rpx;
    align-items: center;
    height: 100%;
    position: relative;
}

.ic-menu-chose {
    margin-left: 20rpx;
    width: 17rpx;
    height: 17rpx;
}

.item-menu-options {
    position: absolute;
    left: 0;
    right: 0;
    top: 50rpx;
    display: flex;
    z-index: 50;
    flex-direction: column;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%;
}

.item-menu-options-content {
    width: 100%;
    display: block;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    font-size: 27rpx;
}

.item {
    flex-direction: column;
    background: #fff;
    margin-top: 20rpx;
}

.item,.item-head {
    display: flex;
}

.item-head {
    height: 60rpx;
    align-items: center;
    width: 100%;
}

.item-month {
    margin-left: 35rpx;
    font-size: 26rpx;
    line-height: 26rpx;
    color: #000;
}

.item-date {
    font-size: 25rpx;
    color: #727272;
    margin-left: 31rpx;
    display: block;
}

.item-body {
    display: flex;
    height: 103rpx;
    border-top: 1rpx solid #efefef;
    width: 100%;
}

.item-body-line {
    height: 100%;
    width: 1rpx;
    background: #efefef;
}

.item-body-part {
    flex: 1;
    display: flex;
    align-items: center;
    height: 100%;
    position: relative;
}

.item-title {
    margin-left: 79rpx;
}

.item-num,.item-title {
    font-size: 28rpx;
    color: #000;
}

.item-num {
    margin-left: 7rpx;
    display: block;
    flex: 1;
}

.item-more {
    width: 14rpx;
    height: 25rpx;
    margin-right: 29rpx;
}