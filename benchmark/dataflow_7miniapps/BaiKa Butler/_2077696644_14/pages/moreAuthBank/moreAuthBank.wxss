.tip {
    display: block;
    min-height: 70rpx;
    text-align: center;
    font-size: 26rpx;
    line-height: 70rpx;
    background: #fff7e4;
    color: #000;
}

.head {
    width: 100%;
    position: relative;
    display: flex;
    vertical-align: top;
    background: #fff;
    box-shadow: 0 15rpx 20rpx -20rpx var(--theme-color);
}

.item-title {
    display: inline-block;
    margin-left: 44rpx;
    height: 78rpx;
    font-size: 27rpx;
    color: #000;
    text-align: center;
    line-height: 78rpx;
}

.item-body {
    display: inline-flex;
    width: 510rpx;
    margin-left: 90rpx;
    font-size: 26rpx;
    flex-flow: wrap;
}

.item-channel {
    position: relative;
    width: 120rpx;
    margin-top: 15rpx;
    margin-bottom: 15rpx;
    display: inline-block;
    margin-right: 43rpx;
    font-size: 23rpx;
    color: #fff;
    text-align: center;
    height: 48rpx;
    border-radius: 10rpx;
    background: #ccc;
    line-height: 48rpx;
}

.item-channel-progress {
    position: absolute;
    width: 70rpx;
    top: 0;
    left: 0rpx;
    border-radius: 10rpx;
    height: 100%;
    background: #6faa4e;
}

.item-channel-num {
    position: absolute;
    display: block;
    width: 100%;
    height: 100%;
    z-index: 10;
}

.item-channel-check {
    width: 28rpx;
    height: 28rpx;
    position: absolute;
    right: -3rpx;
    z-index: 10;
    bottom: -3rpx;
}

.more-channel {
    width: 100%;
    height: 60rpx;
}

.btn-large-opening {
    width: 700rpx;
    height: 85rpx;
    margin: 20rpx 25rpx;
    border-radius: 12rpx;
}

.modal-large-opening-box {
    position: fixed;
    z-index: 999;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.5);
}

.modal-large-opening {
    width: 530rpx;
    margin: 40% 110rpx 0;
    background-size: 100% auto;
}

.modal-large-opening-title {
    font-size: 40rpx;
    font-weight: 700;
    color: #fff;
    text-align: center;
    line-height: 100rpx;
}

.modal-large-opening-content {
    font-size: 30rpx;
    text-align: center;
    line-height: 50rpx;
    color: #353535;
    margin: 60rpx;
}

.modal-large-opening-btn {
    justify-content: space-between;
    margin: 50rpx 20rpx 40rpx;
}

.modal-large-opening-btn button {
    width: 180rpx;
    color: #999;
    font-size: 32rpx;
    line-height: 64rpx;
    background: #e6e6e6;
    border-radius: 50rpx;
}

.modal-large-opening-btn button:last-child {
    color: #fff;
    background: #ffb204;
}

.modal-large-opening-btn {
    display: flex;
    justify-content: space-around;
}

.beMember {
    color: #895f00;
    font-size: 28rpx;
    line-height: 65rpx;
    text-align: center;
    background: #ffe6af;
}

.scroll-body {
    height: 100%;
    width: 100%;
    margin-bottom: 80rpx;
}

.item {
    display: flex;
    position: relative;
    margin-left: 25rpx;
    margin-top: 25rpx;
    width: 700rpx;
    height: 283rpx;
    background-image: url("data:image/png;base64,");
}

.item,.item-bank-info {
    background-size: 100% 100%;
}

.item-bank-info {
    color: #fff;
    display: inline-flex;
    width: 145rpx;
    height: 100%;
    flex-direction: column;
    align-items: center;
    font-size: 28rpx;
}

.item-bank-bg-c {
    background-image: url("data:image/png;base64,");
}

.item-bank-bg-a {
    background-image: url("data:image/png;base64,");
}

.item-bank-bg-b {
    background-image: url("data:image/png;base64,");
}

.item-bank-bg-d {
    background-image: url("data:image/png;base64,");
}

.item-bank-bg-e {
    background-image: url("data:image/png;base64,");
}

.item-bank-icon {
    width: 65rpx;
    height: 65rpx;
    display: block;
    margin: 67rpx 0 20rpx;
    border-radius: 50%;
}

.item-rebind {
    width: 190rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    font-weight: 700;
}

.item-other {
    flex: 1;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.item-no-support {
    font-size: 35rpx;
}