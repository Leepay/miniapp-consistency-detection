.head {
    height: 84rpx;
    background: #fff;
}

.head,.search-phone-body {
    display: flex;
    align-items: center;
}

.search-phone-body {
    width: 301rpx;
    height: 50rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    background: #f1f1f3;
    border-radius: 5rpx;
}

.ic-search {
    width: 30rpx;
    height: 30rpx;
    margin-left: 10rpx;
}

.input-search-phone {
    border: none;
    margin-left: 21rpx;
    font-size: 27rpx;
    color: #000;
}

.search-picker {
    height: 50rpx;
    flex: 1;
}

.search-body {
    width: 100%;
    display: flex;
    height: 50rpx;
    align-items: center;
}

.search-title {
    font-size: 24rpx;
    color: #2e2e2e;
}

.ic-search-type {
    width: 12rpx;
    height: 8rpx;
    margin-left: 7rpx;
    margin-right: 7rpx;
}

.search-option {
    font-size: 25rpx;
    padding-left: 10rpx;
    color: #000;
}

.menu {
    margin-top: 41rpx;
    display: flex;
    height: 50rpx;
    line-height: 50rpx;
    align-items: center;
}

.ic-menu {
    display: block;
    width: 15rpx;
    margin-left: 28rpx;
    height: 26rpx;
}

.member-num {
    font-size: 26rpx;
    margin-left: 17rpx;
}

.body {
    position: fixed;
    width: 100%;
    top: 171rpx;
    bottom: 0rpx;
}

.scroll-list {
    height: 100%;
    width: 100%;
}

.item {
    margin: 30rpx 28rpx;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
    height: 246rpx;
    border-radius: 10rpx;
    flex-direction: column;
}

.item,.item-head {
    display: flex;
}

.item-head {
    width: 100%;
    height: 68rpx;
    line-height: 68rpx;
    align-items: center;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.item-phone {
    flex: 1;
    display: inline-block;
    height: 100%;
    font-size: 30rpx;
    color: #000;
    margin-left: 48rpx;
}

.item-time {
    font-size: 24rpx;
    color: #454545;
    margin-right: 41rpx;
}

.item-body {
    height: 107rpx;
    width: 100%;
}

.item-body,.item-body-part {
    display: flex;
}

.item-body-part {
    flex: 1;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.item-body-title {
    font-size: 26rpx;
    color: #6a6a6a;
}

.item-body-num {
    margin-top: 10rpx;
    font-size: 26rpx;
    color: #000;
}

.item-footer {
    height: 71rpx;
    line-height: 71rpx;
    font-size: 26rpx;
    width: 100%;
    display: flex;
}

.item-footer-title {
    display: inline-block;
    margin-left: 50rpx;
    margin-right: 40rpx;
}

.item-remark {
    flex: 1;
    width: 300rpx;
    overflow: hidden;
    display: inline-block;
    margin-right: 20rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.item-more {
    margin-right: 39rpx;
    font-size: 24rpx;
    display: inline-block;
    color: #60a213;
}