.bg {
    position: absolute;
    z-index: -1;
    top: 0;
    left: -100rpx;
    right: -100rpx;
    height: 480rpx;
    border-radius: 0 0 40% 40%;
    background: linear-gradient(var(--theme-color),var(--theme-color-light));
}

.home-head {
    margin: 180rpx 20rpx 0;
    height: 350rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0 0 30rpx -10rpx var(--theme-color);
}

.head-bg {
    width: 195rpx;
    height: 191rpx;
    position: absolute;
    right: 0;
    bottom: 0;
}

.head-top {
    margin-top: 63rpx;
    margin-left: 75rpx;
    width: 608rpx;
    height: 135rpx;
    position: relative;
    display: flex;
    flex-direction: column;
}

.head-top .money-tip {
    font-size: 24rpx;
}

.head-top .money-num {
    font-size: 72rpx;
}

.head-part {
    position: absolute;
    top: 210rpx;
    left: 80rpx;
    width: 630rpx;
    display: flex;
}

.head-half {
    height: 100%;
    position: relative;
    display: inline-flex;
    flex-direction: column;
}

.head-half:first-child {
    width: 35%;
    margin-right: 100rpx;
    border-right: 3rpx dashed #555;
}

.head-title {
    font-size: 24rpx;
    color: #555;
    margin-bottom: 12rpx;
}

.head-num {
    display: block;
    line-height: 45rpx;
    font-size: 45rpx;
    height: 45rpx;
}

.head-num-part {
    display: flex;
    height: 45rpx;
    align-items: center;
}

.head-notification {
    position: absolute;
    top: -10rpx;
    left: 0;
    right: 0;
    background: #c7e5ff;
    height: 56rpx;
}

.head-notification-body {
    height: 56rpx;
    align-items: center;
    width: 711rpx;
    display: flex;
    flex-direction: row;
}

.ic-notification {
    width: 30rpx;
    height: 30rpx;
    margin-left: 12rpx;
}

.notification-title {
    margin-left: 13rpx;
    font-size: 25rpx;
    color: #545454;
}

.notification-content {
    margin-left: 30rpx;
    width: 500rpx;
    font-size: 25rpx;
    color: #000;
    display: block;
    line-height: 30rpx;
    height: 30rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.ic-more-notification {
    margin-right: 17rpx;
    width: 25rpx;
    height: 25rpx;
}

.body {
    position: absolute;
    width: 100%;
    top: 550rpx;
    bottom: 0rpx;
}

.repay-body {
    position: absolute;
    right: 0;
    top: 0;
    display: flex;
    align-items: center;
}

.ic-repaying {
    display: inline-block;
    width: 36rpx;
    height: 33rpx;
}

.ic-repaying-tip {
    display: inline-block;
    width: 32rpx;
    height: 16rpx;
}

.repaying-tip {
    margin-left: 27rpx;
    color: #d21515;
    font-size: 30rpx;
    letter-spacing: 2rpx;
}

.scroll-list {
    height: 100%;
}

.item-parent {
    position: relative;
    width: 710rpx;
    margin-left: 20rpx;
    margin-bottom: 20rpx;
    border-radius: 20rpx;
    background: #fff;
    overflow: hidden;
}

.img-stamp-pause {
    position: absolute;
    z-index: 80;
    width: 153rpx;
    height: 108rpx;
    top: 150rpx;
    left: 100rpx;
}

.item-card {
    height: 310rpx;
    width: 100%;
    display: flex;
    align-items: center;
}

.item-card-lock {
    background: #bbb;
}

.lockCard {
    text-align: center;
    color: #353535;
    margin: 50rpx 0 60rpx;
}

.lockCard view {
    width: 120rpx;
    height: 120rpx;
    line-height: 120rpx;
    margin: 0 auto 50rpx;
    border: 1px solid #ccc;
    border-radius: 100%;
    color: #4acc59;
    font-size: 80rpx;
    transform: rotate(180deg);
}

.item-bank-div {
    display: inline-flex;
    flex-direction: column;
    align-items: center;
    width: 148rpx;
    height: 100%;
}

.item-bank-icon {
    width: 56rpx;
    height: 56rpx;
    margin-top: 73rpx;
    border-radius: 50%;
}

.item-bank-num {
    margin-top: 20rpx;
}

.item-bank-name,.item-bank-num {
    font-size: 24rpx;
    line-height: 24rpx;
}

.item-bank-name {
    margin-top: 15rpx;
}

.item-edit {
    width: 90rpx;
    text-align: center;
    font-size: 22rpx;
    color: #cb1818;
    margin-top: 20rpx;
    padding: 4rpx 8rpx;
    line-height: 26rpx;
    background: #ffecec;
}

.item-edit.false {
    color: #8a1c1c;
    background: #aa9d9d;
}

.item-bank-dashed {
    display: inline-block;
    width: 1rpx;
    height: 150rpx;
}

.item-card-body {
    display: flex;
    flex-direction: column;
    flex: 1;
    margin-left: 50rpx;
    height: 100%;
    color: #444;
    position: relative;
}

.card-body-top {
    height: 150rpx;
    position: relative;
    width: 100%;
}

.item-card-repay {
    width: 100%;
    height: 100%;
    display: flex;
    position: relative;
}

.item-card-repay-tip {
    font-size: 20rpx;
    color: #595757;
    margin-top: 50rpx;
    line-height: 20rpx;
    width: 160rpx;
    display: block;
}

.item-card-repay-money {
    min-width: 160rpx;
}

.item-card-repay-big-money,.item-card-repay-money {
    font-size: 66rpx;
    display: block;
    line-height: 58rpx;
    position: absolute;
    left: -8rpx;
    bottom: 0;
    color: #333;
}

.item-card-repay-b {
    position: absolute;
    bottom: 0;
    display: flex;
    right: 80rpx;
}

.item-countdown-time {
    font-size: 66rpx;
    line-height: 70rpx;
}

.item-countdown-time-desc {
    font-size: 20rpx;
    margin-left: 2rpx;
    line-height: 30rpx;
    margin-top: 8rpx;
}

.item-menu-position {
    position: absolute;
    top: 0rpx;
    z-index: 40;
    right: 0;
    border-radius: 0 20rpx;
}

.item-menu {
    margin: 0rpx;
    padding: 0;
    display: inline-block;
    width: 143rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    color: #fff;
    font-size: 26rpx;
    font-family: PingFangSC-Light,sans-serif;
}

.item-menu,.item-menu::after {
    border: none;
}

.card-body-bottom {
    width: 100%;
    display: flex;
    margin-top: 40rpx;
    justify-content: space-between;
}

.item-bottom-tip {
    min-width: 160rpx;
    display: block;
    font-size: 20rpx;
    line-height: 20rpx;
}

.item-bottom-num {
    min-width: 160rpx;
    display: block;
    margin-top: 16rpx;
    font-size: 34rpx;
    line-height: 34rpx;
}

.edit-bill-tip-img {
    position: absolute;
    width: 100%;
    height: 310rpx;
    top: 0;
}

.have-channel-flag {
    line-height: 66rpx;
    font-size: 28rpx;
    text-align: center;
    color: #cb1818;
    background: #ffdede;
}

.have-channel-flag image {
    width: 24rpx;
    height: 24rpx;
}

.item-lockStatus-tip {
    position: absolute;
    font-weight: 700;
    left: 160rpx;
    top: 130rpx;
    text-align: center;
    width: 400rpx;
    font-size: 32rpx;
    color: #d21515;
}

.bottom {
    height: 200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.img-add-card {
    height: 90rpx;
    width: 90rpx;
    display: block;
    margin-top: 30rpx;
}

.text-add-card {
    color: #000;
    font-size: 34rpx;
    margin-top: 10rpx;
}

.img-modal-delete {
    height: 80rpx;
    width: 80rpx;
    display: block;
    margin: auto auto 10rpx;
}

.body-lock {
    width: 100%;
    height: 100%;
    position: relative;
    z-index: 80;
}

.btn-lock {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 40;
    border-radius: 20rpx 0;
}

.img-bill-tip {
    display: block;
    width: 482rpx;
    height: 136rpx;
}

.img-bind-tip {
    width: 328rpx;
    height: 242rpx;
    margin: auto;
}

.item-rebind-channel {
    font-size: 24rpx;
    position: absolute;
    width: 200rpx;
    padding: 0;
    margin: 0;
    height: 60rpx;
    top: 103rpx;
    left: 265rpx;
}

.mask-body {
    font-size: 31rpx;
    color: #323232;
    padding: 60rpx;
}

.mask-tip {
    font-size: 25rpx;
    display: block;
    width: 420rpx;
    padding: 25rpx;
    background: #f2f2f2;
    color: #7b7b7b;
    margin-bottom: 40rpx;
}

.btn-fast-use {
    display: block;
    margin: 50rpx auto;
    width: 410rpx;
    height: 96rpx;
    font-size: 28rpx;
    text-align: center;
    line-height: 96rpx;
    border-radius: 48rpx;
    color: #fff;
    background: var(--theme-color);
}

.btn-fast-use:active {
    opacity: .8;
}

.welcome-tip {
    background: #fff;
    width: 532rpx;
}

.welcome-tip>image {
    width: 532rpx;
    height: 188rpx;
    margin-top: -50rpx;
}

.welcome-tip button {
    width: 409rpx;
    height: 86rpx;
    line-height: 86rpx;
    font-size: 30rpx;
    margin: 36rpx auto;
    color: #fff;
    position: relative;
}

.welcome-tip button image {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
}

.welcome-tip button text {
    position: absolute;
    width: 100%;
    z-index: 9;
    left: 0;
}

.welcome-tip view {
    line-height: 88rpx;
    margin-bottom: -50rpx;
    background: #e7e7e7;
    text-align: center;
    border-radius: 0 0 20rpx 20rpx;
    font-size: 30rpx;
    color: #4f4f4f;
}