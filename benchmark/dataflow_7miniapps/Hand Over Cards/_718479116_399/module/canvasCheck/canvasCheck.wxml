<view class="make-con" wx:if="{{!pageOrigin}}">
    <image animation="{{starShow}}" class="make-img" src="{{buildSucImg}}" style="opacity:0;"></image>
    <view class="canvasBox">
        <canvas canvasId="runCanvasS" class="canvas" id="runCanvasS" wx:if="{{showCircle}}"></canvas>
        <image class="checkMark" src="/images/checkMark.jpg"></image>
        <view animation="{{cmShadeAni}}" class="cmShade"></view>
    </view>
</view>
<view class="make-con" wx:if="{{pageOrigin=='stickSuc'}}">
    <image class="stickSucImg" src="/images/cdetails/stickSucTopImg.png"></image>
    <image animation="{{starShow}}" class="make-img new-make-img" src="{{buildSucImg}}" style="opacity:0;"></image>
    <view class="canvasBox new-canvasBox">
        <canvas canvasId="runCanvasS" class="canvas new-canvas" id="runCanvasS" wx:if="{{showCircle}}"></canvas>
        <image class="checkMark new-checkMark" src="/images/checkMark.jpg"></image>
        <view animation="{{cmShadeAni}}" class="cmShade new-cmShade"></view>
    </view>
</view>
<view class="make-con make-con-cs" wx:if="{{pageOrigin=='changeSuc'}}">
    <view class="arrow-cs"></view>
    <view class="text-cs">名片交换成功</view>
    <image animation="{{starShow}}" class="make-img make-img-cs" src="{{buildSucImg}}" style="opacity:0;"></image>
    <view class="canvasBox canvasBox-cs">
        <canvas canvasId="runCanvasS" class="canvas canvas-cs" id="runCanvasS" wx:if="{{showCircle}}"></canvas>
        <image class="checkMark checkMark-cs" src="/images/checkMark.jpg"></image>
        <view animation="{{cmShadeAni}}" class="cmShade cmShade-cs"></view>
    </view>
</view>
