page {
    overflow-y: auto;
    background-color: #fff;
}

.build {
    position: fixed;
    width: 100%;
    height: 100%;
    padding-bottom: 30rpx;
    overflow-x: hidden;
    background: #1e232f;
}

.build_title {
    height: 44rpx;
    font-size: 32rpx;
    display: flex;
    font-weight: 700;
    color: #333;
    line-height: 44rpx;
    padding-left: 20rpx;
}

.searchButton .clear {
    clear: both;
}

.build .propaganda .hander {
    height: 66rpx;
    font-size: 48rpx;
    font-weight: 500;
    color: #fad7ab;
    text-align: center;
    line-height: 66rpx;
}

.build .propaganda .title {
    height: 34rpx;
    font-size: 24rpx;
    color: #fad7ab;
    padding-top: 4rpx;
    text-align: center;
    line-height: 34rpx;
}

.build .propaganda .overflow {
    height: 80rpx;
    width: 314rpx;
    display: block;
    margin: 0 auto;
    overflow: hidden;
    padding-top: 22rpx;
}

.build .propaganda .propagandaBg {
    height: 112rpx;
    width: 314rpx;
    display: block;
    margin: 0 auto;
}

.build .searchButton {
    margin: 0 20rpx 0 34rpx;
    padding-top: 10rpx;
}

.build .content {
    background: #fff;
    box-shadow: 2rpx 2rpx 10rpx 0rpx rgba(44,44,44,.26);
    border-radius: 20rpx;
    margin: 30rpx 20rpx 0;
    overflow: hidden;
}

.build .content .intor {
    display: none;
}

.intorList {
    display: flex;
    float: left;
    height: 34rpx;
    padding-top: 20rpx;
    margin-left: 20rpx;
    text-align: center;
}

.intorList text {
    height: 34rpx;
    font-size: 24rpx;
    color: #fff;
    line-height: 34rpx;
    opacity: .7;
}

.intorList image {
    width: 58rpx;
    height: 7rpx;
    padding-top: 14rpx;
}

.intorList .listStyle {
    width: 6rpx;
    height: 6rpx;
    margin: auto 10rpx;
    border-radius: 50%;
    opacity: .8;
    background: #fff;
}

.build_title image {
    width: 48rpx;
    height: 42rpx;
    margin-top: 13rpx;
    margin-right: 13rpx;
}

.build_intro {
    height: 34rpx;
    font-size: 24rpx;
    color: #999;
    line-height: 34rpx;
    padding-left: 20rpx;
    padding-top: 10rpx;
    margin-bottom: 20rpx;
}

.intro_con {
    padding-left: 20rpx;
    height: 128rpx;
}

.intro_con view {
    float: left;
    min-width: 96rpx;
    padding: 0 8rpx;
    height: 44rpx;
    background: rgba(41,121,255,.1);
    border-radius: 4rpx;
    margin-right: 20rpx;
    margin-top: 20rpx;
    font-size: 24rpx;
    color: #2979ff;
    text-align: center;
}

.intor {
    padding: 0 20rpx!important;
}

.intro_item5 {
    line-height: 34rpx;
}

.inputStyle {
    height: 42rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: #333;
    line-height: 42rpx;
}

.select-style {
    display: none;
    margin-top: 20rpx;
    padding: 0 20rpx;
}

.select-style .title {
    margin-bottom: 20rpx;
    font-size: 32rpx;
    font-weight: 700;
    color: #333;
    line-height: 44rpx;
}

.select-style .card {
    display: flex;
}

.select-style .card-img {
    width: 344rpx;
    height: 208rpx;
    border-radius: 8rpx;
    border: 3px solid transparent;
}

.select-style .card-img:first-child {
    margin-right: auto;
    margin-left: 0;
}

.select-style .card-img:last-child {
    margin-right: 0;
    margin-left: auto;
}

.select-style .active {
    border: 3px solid #2979ff;
}

.select-style .tip {
    margin-top: 20rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #999;
    line-height: 28rpx;
}

.company {
    margin-top: 60rpx;
    font-size: 40rpx;
    font-weight: 700;
    color: #333;
    text-align: center;
    line-height: 56rpx;
}

.build_input {
    height: 82rpx;
    background: #fff;
    border-radius: 8rpx;
    border: 2rpx solid #dcdfe6;
    border-radius: 16rpx;
    margin: 30rpx 30rpx 80rpx;
    position: relative;
}

.build_input input {
    padding: 20rpx 0rpx 20rpx 20rpx;
    height: 42rpx;
    width: 620rpx;
}

.holderStyle {
    height: 42rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: #cacaca;
    line-height: 42rpx;
}

.icon_clear {
    position: absolute;
    right: 0rpx;
    height: 30rpx;
    width: 30rpx;
    padding: 20rpx;
    top: 6rpx;
}

.teamTip {
    position: absolute;
    color: #ff3b2f;
    font-size: 24rpx;
    line-height: 28px;
}

.buttonAdd {
    width: 372rpx;
    height: 92rpx;
    line-height: 92rpx;
    border-radius: 47rpx;
    margin: 20rpx auto;
    font-size: 32rpx;
    color: #fff;
    font-weight: 700;
    box-shadow: 12rpx 12rpx 20rpx 0rpx rgba(41,46,59,.17);
}

.reminder {
    height: 76rpx;
    padding: 20rpx;
    font-size: 24rpx;
    color: #fff;
    opacity: .7;
    line-height: 38rpx;
}

view .buttonAddDisabled {
    box-shadow: none;
}

view .button_primary {
    color: #fff;
    background: linear-gradient(128deg,#438fff,#256de6);
}

view .button_disabled {
    color: #fff;
    background: #e1e1e1;
}

.editCardPageMatch {
    position: absolute;
    width: 710rpx;
    margin-left: 20rpx;
    height: 200px;
    opacity: 1;
    z-index: 9999;
}

.editCardPageMatch_item {
    background-color: #fff;
    color: #000;
}

.editCardPageMatch_item_test {
    height: 35px;
    line-height: 35px;
    padding: 2rpx 20rpx;
    max-width: 96%;
}

.editCardPageMatch_item_test2 {
    background-color: #d5d5d5;
    height: 2rpx;
}

.covered {
    display: flex;
    justify-content: center;
    padding: 12rpx 0 78rpx;
    font-size: 24rpx;
    color: #f2a72c;
}

.covered image {
    width: 28rpx;
    height: 36rpx;
    margin-top: 4rpx;
    margin-right: 10rpx;
}