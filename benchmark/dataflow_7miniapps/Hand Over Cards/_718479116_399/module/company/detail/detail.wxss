.d-panel {
    position: fixed;
    z-index: 121;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.75);
}

.d-panel,.d-panel .d-container {
    display: flex;
    flex-direction: column;
}

.d-panel .d-container {
    position: relative;
    box-sizing: border-box;
    padding: 0 10rpx;
    margin: auto;
    width: 646rpx;
    min-height: 536rpx;
    background: #fff;
    border-radius: 20rpx;
    text-align: center;
}

.d-panel .close {
    position: absolute;
    right: 0;
    top: 0;
    padding: 20rpx;
    width: 24rpx;
    height: 24rpx;
}

.d-panel .clock {
    margin: 60rpx auto 20rpx;
    width: 64rpx;
    height: 64rpx;
}

.d-panel .d-container .t {
    font-size: 36rpx;
    font-weight: 700;
    color: #333;
    margin: 0 auto;
}

.d-panel .d-container .i,.d-panel .d-container .i2 {
    margin-top: 20rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #666;
    line-height: 40rpx;
}

.d-panel .d-container .i2 {
    line-height: 44rpx;
}

.d-panel .d-container .s {
    display: block;
    width: 500rpx;
    height: 300rpx;
    margin: 20rpx auto 0;
}

.d-panel .d-content {
    font-size: 28rpx;
    color: #666;
    line-height: 40rpx;
    margin-top: 40rpx;
    text-align: left;
    padding-left: 60rpx;
}

.d-panel .d-content view {
    max-width: 520rpx;
}

.d-panel .d-content .ct text {
    margin-left: 20rpx;
    background: #f7b51c;
    border-radius: 4rpx;
    color: #fff;
    padding: 6rpx 8rpx;
    font-weight: 700;
    font-size: 24rpx;
}

.d-panel .d-content .ci,.d-panel .d-content .ci2 {
    margin-top: 20rpx;
}

.d-panel .d-content .ci {
    line-height: 44rpx;
}

.d-panel .d-content-n {
    font-size: 28rpx;
    color: #666;
    line-height: 40rpx;
    margin-top: 40rpx;
    text-align: left;
    padding-left: 60rpx;
}

.d-panel .d-content-n .ct text {
    margin-left: 20rpx;
    background: #09bb07;
    border-radius: 4rpx;
    color: #fff;
    padding: 6rpx 8rpx;
    font-weight: 700;
    font-size: 24rpx;
}

.d-panel .d-content-n .ci {
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.d-panel .d-content-n .ci text {
    margin-left: 10rpx;
}

.d-panel .d-content-n2 {
    font-size: 28rpx;
    color: #666;
    line-height: 40rpx;
    margin-top: 40rpx;
    text-align: left;
    padding-left: 60rpx;
}

.d-panel .d-content-n2 .ct text {
    margin-left: 20rpx;
    background: #fa5151;
    border-radius: 4rpx;
    color: #fff;
    padding: 6rpx 8rpx;
    font-weight: 700;
    font-size: 24rpx;
}

.d-panel .d-content-n2 .ci {
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.d-panel .d-content-n2 .ci text {
    margin-left: 10rpx;
    text-decoration: line-through;
}

.d-panel .d-container .btn {
    margin: 70rpx auto 80rpx;
    font-size: 32rpx;
    font-weight: 700;
    color: #fff;
    width: 342rpx;
    height: 92rpx;
    line-height: 92rpx;
    background: linear-gradient(128deg,#438fff,#256de6);
    box-shadow: 12rpx 12rpx 20rpx 0 rgba(41,46,59,.17);
    border-radius: 47rpx;
}

.d-panel .d-container .t2 {
    margin-top: -60rpx;
    margin-bottom: 20rpx;
    height: 40rpx;
    font-size: 24rpx;
    color: #999;
    line-height: 40rpx;
    font-weight: 400;
}

page {
    overflow-x: hidden;
    background-color: #ecedef;
}

.zIndex {
    z-index: 121;
}

.team_bold {
    font-weight: 700;
}

.viewEllipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.company-detail {
    position: relative;
    padding: 0;
}

.company-detail .company-detail-show {
    background-color: #2979ff;
    color: #fff;
    display: flex;
    align-items: center;
    padding-bottom: 64rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    position: relative;
}

.company-detail .company-show-name {
    max-width: 504rpx;
    margin-right: 6rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.company-detail-show .company-show-tag {
    padding: 0 8rpx;
    border-radius: 4rpx;
    font-size: 24rpx;
    color: #fff;
    line-height: 40rpx;
    height: 40rpx;
}

.company-detail .company-detail-show mini-icon {
    position: absolute;
    right: 20rpx;
    padding: 20rpx;
}

.company-detail .company-detail-server {
    position: relative;
    background: linear-gradient(135deg,#1e232f,#252e45);
    border-radius: 12rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    margin-top: -42rpx;
    padding: 24rpx 20rpx;
    overflow: hidden;
}

.company-detail .company-server-back {
    width: 100%;
    height: 88rpx;
    position: absolute;
    top: 0;
    left: 0;
    border-radius: 12rpx;
    z-index: 0;
}

.company-detail .company-server-title {
    height: 36rpx;
    width: 186rpx;
    position: relative;
    z-index: 1;
    display: block;
}

.company-detail .company-show-tag {
    position: absolute;
    left: 216rpx;
    top: 24rpx;
    padding: 0 8rpx;
    border-radius: 4rpx;
    font-size: 24rpx;
    color: #fff;
    line-height: 40rpx;
    height: 40rpx;
}

.company-detail .company-server-pay {
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    font-size: 24rpx;
    color: #fad7ab;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.company-detail .pay-active {
    color: #fad7ab;
    opacity: .7;
}

.company-detail .pay-overDate {
    color: #fa5151;
}

.company-detail .company-server-tip {
    position: relative;
    z-index: 1;
    height: 34rpx;
    font-size: 24rpx;
    color: #fad7ab;
    line-height: 34rpx;
    margin-top: 10rpx;
    opacity: .5;
}

.company-detail .company-server-tip2 {
    position: absolute;
    height: 34rpx;
    font-size: 24rpx;
    color: #fad7ab;
    line-height: 34rpx;
    margin-top: 5rpx;
    top: 64rpx;
    right: 20rpx;
    opacity: .5;
}

.company-detail .company-server-tip3 {
    height: 34rpx;
    font-size: 24rpx;
    color: #fa5151;
    line-height: 34rpx;
    margin-top: 10rpx;
    position: relative;
    z-index: 1;
}

.company-detail .company-detail-member {
    margin: 20rpx;
    background: #fff;
    border-radius: 12rpx;
    padding: 30rpx 20rpx 38rpx;
    position: relative;
}

.company-detail .company-member-title {
    height: 44rpx;
    font-size: 32rpx;
    line-height: 44rpx;
    padding-bottom: 30rpx;
}

.company-detail .company-member-contain {
    flex-direction: row;
    justify-content: space-between;
}

.company-detail .company-member-contain,.company-detail .company-member-item {
    display: flex;
    align-items: center;
}

.company-detail .company-member-item {
    flex-direction: column;
    justify-content: center;
    font-size: 28rpx;
    color: #666;
    line-height: 40rpx;
    height: auto;
    width: auto;
    background-color: transparent;
}

.company-detail .company-member-number {
    height: 60rpx;
    max-width: 100rpx;
    font-size: 56rpx;
    color: #2979ff;
    line-height: 60rpx;
}

.company-detail .all-member {
    border-left: 2px solid #eceeea;
    padding-left: 39rpx;
}

.company-detail .company-member-item image {
    height: 60rpx;
    width: 60rpx;
    margin-bottom: 4rpx;
}

.company-detail .company-member-add {
    position: absolute;
    right: 20rpx;
    top: 50rpx;
    height: 42rpx;
    background: #fa5151;
    border-radius: 21rpx;
}

.company-detail .company-member-add button {
    display: block;
    height: 42rpx;
    line-height: 42rpx;
    background-color: transparent;
    border-radius: 21rpx;
    padding: 0 14rpx;
    font-size: 24rpx;
    color: #fff;
}

.company-detail .company-member-add::after {
    position: absolute;
    content: "";
    bottom: -18rpx;
    right: 44rpx;
    height: 0;
    width: 0;
    border: 10rpx solid transparent;
    border-top-color: #fa5151;
}

.company-detail .company-detail-style {
    margin: 20rpx;
    background: #fff;
    border-radius: 12rpx;
    padding: 30rpx 20rpx 38rpx;
}

.company-detail .company-style-title {
    height: 44rpx;
    font-size: 32rpx;
    line-height: 44rpx;
    padding-bottom: 30rpx;
}

.company-detail .company-style-contain {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.company-detail .company-style-contain image {
    width: 230rpx;
    max-width: 230rpx;
    height: 138rpx;
    margin-right: 20rpx;
    position: relative;
}

.company-detail .company-style-shade::after {
    position: absolute;
    content: "";
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
    background-color: hsla(0,0%,88.2%,.8);
}

.company-detail .company-style-right {
    width: 458rpx;
}

.company-detail .style-right-title {
    height: 32rpx;
    font-size: 28rpx;
    color: #666;
    line-height: 32rpx;
}

.company-detail .style-right-tip {
    font-size: 24rpx;
    color: #999;
    line-height: 38rpx;
    margin-top: 10rpx;
}

.company-detail .company-detail-helper {
    margin: 20rpx;
    background: #fff;
    border-radius: 12rpx;
    padding: 30rpx 20rpx 38rpx;
}

.company-detail .company-helper-title {
    height: 44rpx;
    font-size: 32rpx;
    line-height: 44rpx;
    padding-bottom: 30rpx;
}

.company-detail .company-helper-contain {
    flex-direction: row;
    justify-content: space-between;
}

.company-detail .company-helper-contain,.company-detail .company-helper-item {
    display: flex;
    align-items: center;
}

.company-detail .company-helper-item {
    flex-direction: column;
    justify-content: center;
    font-size: 28rpx;
    color: #666;
    line-height: 40rpx;
}

.company-detail .company-helper-item image {
    height: 60rpx;
    width: 60rpx;
    margin-bottom: 4rpx;
}

.company-detail .company-helper-more {
    font-size: 24rpx;
    color: #999;
    line-height: 38rpx;
    margin-top: 20rpx;
}

.company-bottom {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: #666;
    padding-bottom: 40rpx;
}

.company-bottom image {
    width: 28rpx;
    height: 28rpx;
}

.company-bottom text {
    margin-left: 4rpx;
    margin-right: 4rpx;
}