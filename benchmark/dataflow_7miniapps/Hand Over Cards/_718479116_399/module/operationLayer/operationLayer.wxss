}

@-webkit-keyframes moveLR {
    0% {
        opacity: .25;
        left: -3%;
    }

    3% {
        left: 0;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

@keyframes moveLR {
    0% {
        opacity: .25;
        left: -3%;
    }

    3% {
        left: 0;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

.bottomButton .lineMove {
    position: absolute;
    top: 0rpx;
    height: 120rpx;
    margin-top: -20rpx;
    left: 0rpx;
    width: 30rpx;
    background-color: #fff;
    filter: blur(16rpx);
    transform: rotate(-25deg);
    animation: moveLR 2s linear infinite;
}

