.pageShade {
    position: fixed;
    z-index: 2;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

@-webkit-keyframes loading-in {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

@keyframes loading-in {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

.loading-in {
    display: inline-block;
    border: 4rpx solid rgba(0,0,0,.1);
    border-left-color: #fff;
    border-radius: 50%;
    width: 50rpx;
    height: 50rpx;
    animation: loading-in .5s linear infinite;
}

.loading-tip {
    font-size: 24rpx;
    margin-top: 24rpx;
    color: #fff;
}

.loading-con {
    background-color: #000;
    opacity: .6;
    height: 200rpx;
    width: 200rpx;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.viewEllipsis {
    overflow: hidden;
}

.viewEllipsis,.viewEllipsisX {
    text-overflow: ellipsis;
    white-space: nowrap;
}

.viewEllipsisX {
    overflow-x: hidden;
}

.bottomButton {
