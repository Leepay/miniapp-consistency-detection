page {
    height: 100%;
    width: 100%;
    background-color: #fff;
    font-size: 32rpx;
    color: #333;
    line-height: 44rpx;
    overflow-x: hidden;
}

.immersive {
    height: 44px;
    z-index: 10;
    position: fixed;
    top: 0;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    font-size: 14px;
    font-weight: bolder;
}

.immersive .groupTitle {
    font-size: 18px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 290rpx;
}

.immersive .homePage {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 10px;
    padding: 16rpx 20rpx;
}

.immersive .homePage image {
    width: 20rpx;
    height: 32rpx;
}

.immersive .homePage .microvcardlogo {
    width: 38rpx;
    height: 38rpx;
}

.immersive .nullView {
    width: 100rpx;
}

button,view {
    margin: 0;
    padding: 0;
}

view {
    letter-spacing: 0;
}

button {
    font-size: 32rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #333;
    letter-spacing: 0.2rpx;
    border-radius: 6rpx;
}

button::after {
    content: none;
}

.mainPage-button {
    position: relative;
}

.container {
    height: 100%;
    width: 100%;
    box-sizing: border-box;
}

.addShadow {
    box-shadow: 0 0 14rpx 0 hsla(0,0%,73.7%,.5);
}

.addShadowN {
    box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(47,47,47,.5);
}

.addRadius {
    border-radius: 6rpx;
}

.addRadiusBL,.addRadiusBR,.addRadiusTL,.addRadiusTR {
    border-top-right-radius: 6rpx;
}

.addShadowRadius {
    box-shadow: 0 0 14rpx 0 hsla(0,0%,73.7%,.5);
    border-radius: 6rpx;
}

.addShadowRadiusMini {
    box-shadow: 0 0 8rpx 0 hsla(0,0%,73.7%,.5);
    border-radius: 6rpx;
}

.addBorder {
    border: 1px solid #3f51b5;
}

.addInitColor {
    color: #333;
}

.addColor {
    color: #2979ff;
}

.addLetterListColor {
    color: #7986cb;
}

.addLetterListColorAnd {
    color: #999;
}

.addBackColor {
    background-color: #3f51b5;
}

.addBold {
    font-weight: 700;
}

.addBold4 {
    font-weight: 400;
}

.addBold5 {
    font-weight: 500;
}

.addBold6 {
    font-weight: 600;
}

.activeStyle:active {
    background-color: #f8f8f8;
}

.placeholderColor {
    color: #cacaca;
}

.pageShade {
    position: fixed;
    z-index: 2;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

@-webkit-keyframes loading-in {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

@keyframes loading-in {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

.loading-in {
    display: inline-block;
    border: 4rpx solid rgba(0,0,0,.1);
    border-left-color: #fff;
    border-radius: 50%;
    width: 50rpx;
    height: 50rpx;
    animation: loading-in .5s linear infinite;
}

.loading-tip {
    font-size: 24rpx;
    margin-top: 24rpx;
    color: #fff;
}

.loading-con {
    background-color: #000;
    opacity: .6;
    height: 200rpx;
    width: 200rpx;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.viewEllipsis {
    overflow: hidden;
}

.viewEllipsis,.viewEllipsisX {
    text-overflow: ellipsis;
    white-space: nowrap;
}

.viewEllipsisX {
    overflow-x: hidden;
}

.bottomButton {
    width: 95%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 10;
    height: 80rpx;
    padding: 20rpx 2.5%;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.bottomButtonCenter {
    justify-content: center;
}

.bottomButtonDetail {
    padding-bottom: 50rpx;
}

.bottomButton .buttonDefault {
    width: 33%;
    background: #fff;
    border: 1px solid #e3e3e3;
}

.bottomButton .buttonMiddleL {
    width: 48%;
    background: #fff;
    border: 1px solid #e3e3e3;
}

.bottomButton .buttonMiddleR {
    width: 48%;
    color: #fff;
    background: linear-gradient(314deg,#2e40a5,#4457a3);
}

.bottomButton .buttonPrimaryDisabled {
    width: 63%;
    background-color: #e3e3e3;
    color: #999;
}

.bottomButton .buttonPrimary {
    width: 63%;
}

.bottomButton .buttonPrimary,.bottomButton .buttonPrimaryBig {
    background: linear-gradient(314deg,#2e40a5,#4457a3);
    color: #fff;
}

.bottomButton .buttonPrimaryBig {
    width: 100%;
}

.bottomButton .buttonPrimaryBigDisabled {
    width: 100%;
    background-color: #e3e3e3;
    color: #fff;
}

.bottomButton .buttonDefaultBig {
    width: 100%;
    color: #333;
    background: #fff;
    box-shadow: 0 0 4rpx 0 hsla(0,0%,73.7%,.5);
}

.coverView {
    width: 93%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 0;
    height: 80rpx;
    padding: 20rpx 3.5% 50rpx;
    display: flex!important;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background-color: #fff;
}

.coverView .buttonPrimary,.coverView .buttonPrimaryBig {
    background-color: #3949ab;
    border: 1px solid #3949ab;
}

@-webkit-keyframes moveLR {
    0% {
        opacity: .25;
        left: -3%;
    }

    3% {
        left: 0;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

@keyframes moveLR {
    0% {
        opacity: .25;
        left: -3%;
    }

    3% {
        left: 0;
    }

    92% {
        left: 100%;
    }

    100% {
        opacity: 0;
        left: 100%;
    }
}

.bottomButton .lineMove {
    position: absolute;
    top: 0rpx;
    height: 120rpx;
    margin-top: -20rpx;
    left: 0rpx;
    width: 30rpx;
    background-color: #fff;
    filter: blur(16rpx);
    transform: rotate(-25deg);
    animation: moveLR 2s linear infinite;
}

.cardList {
    position: relative;
    margin-top: 20rpx;
    background: #fff;
}

.cardList .cardList-item {
    display: flex;
    flex-direction: row;
    padding: 20rpx 26rpx 30rpx;
}

.cardList .cardList-item .cardList-item-image {
    overflow: hidden;
    position: relative;
    height: 162rpx;
    width: 270rpx;
    border: 1px solid #ebebeb;
}

.cardList .cardList-item .cardList-item-image image {
    width: 100%;
    height: 100%;
}

.cardList .cardList-item .cardList-wxMessage {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 54rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background: rgba(0,0,0,.56);
}

.cardList .cardList-item .cardList-wxMessage image {
    width: 44rpx;
    height: 44rpx;
    border-radius: 44rpx;
    margin: 0 10rpx;
}

.cardList .cardList-item .cardList-wxMessage .cardList-wxMessage-text {
    font-size: 22rpx;
    color: #fff;
    max-width: 198rpx;
}

.cardList .cardList-item .cardList-item-con {
    position: relative;
    width: 52%;
    font-size: 28rpx;
    margin-left: 20rpx;
}

.cardList .cardList-item .cardList-item-con view {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cardList .cardList-item .cardList-item-con text {
    background: #e91e63;
    border-radius: 6rpx;
    color: #fff;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 22rpx;
    padding: 0 8rpx;
    display: inline-block;
    margin-left: 16rpx;
}

.cardList .cardList-image-tip {
    position: absolute;
    left: 0;
    border-top-right-radius: 132rpx;
    border-bottom-right-radius: 132rpx;
    padding: 0 20rpx 0 8rpx;
    bottom: 0;
    height: 44rpx;
    max-width: 100%;
    min-width: 100rpx;
    line-height: 44rpx;
    font-size: 24rpx;
    color: #fff;
    background-color: rgba(0,0,0,.5);
}

.cardList .cardList-item-tip {
    position: absolute;
    right: 0;
    background-color: #e9efff;
    border-top-left-radius: 130rpx;
    border-bottom-left-radius: 130rpx;
    padding: 0 20rpx;
    height: 46rpx;
    font-size: 24rpx;
    color: #3f51b5;
    letter-spacing: 0;
}

.cardList .cardList-item-moren {
    margin-left: 20rpx;
    background-color: #e91e63;
    padding: 8rpx 10rpx;
    font-size: 22rpx;
    color: #fff;
    font-weight: 400;
    letter-spacing: -.07px;
    line-height: 18rpx;
}

.cardList .cardList-item-con-name {
    width: 100%;
    line-height: 52rpx;
    font-size: 38rpx;
    font-weight: 700;
}

.cardList .cardList-item-con-name .cardList-item-con-name-n {
    max-width: 60%;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cardList .cardList-item-con-company {
    width: 100%;
    line-height: 40rpx;
    font-size: 28rpx;
    margin-top: 18rpx;
    color: #666;
}

.cardList .cardList-item-con-job {
    position: absolute;
    font-size: 24rpx;
    line-height: 34rpx;
    bottom: 0;
    color: #666;
    padding: 2rpx 14rpx;
    background-color: #f3f0f0;
    max-width: 275rpx;
}

.netError {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #fff;
}

.netError .netError-image {
    width: 498rpx;
    height: 284rpx;
    display: flex;
    margin: auto;
}

.netError .netError-tip {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #999;
    letter-spacing: -0.018rpx;
    text-align: center;
    margin-top: 9%;
}

.netError .netError-butt {
    background-color: #fff;
    width: 300rpx;
    margin: 13% auto auto;
}

.lowerCanvas {
    width: 50px;
    height: 50px;
    position: fixed;
    left: 100%;
}

.m_color {
    color: #2979ff;
}

.m_border_color {
    border: 1px solid #2979ff;
}

.m_bold {
    font-weight: 700;
}

.m_viewEllipsis {
    overflow: hidden;
}

.m_viewEllipsis,.m_viewEllipsisX {
    text-overflow: ellipsis;
    white-space: nowrap;
}

.m_viewEllipsisX {
    overflow-x: hidden;
}

.m_button_big {
    width: 710rpx;
}

.m_button_big,.m_button_middle {
    height: 92rpx;
    line-height: 92rpx;
    font-size: 32rpx;
    font-weight: 700;
    border-radius: 47rpx;
}

.m_button_middle {
    width: 372rpx;
}

.m_button_mini {
    height: 56rpx;
    line-height: 56rpx;
    width: 172rpx;
    font-size: 24rpx;
    font-weight: 700;
    border-radius: 28rpx;
}

.m_button_primary {
    color: #fff;
    background: linear-gradient(128deg,#438fff,#256de6);
}

.m_button_default {
    color: #333;
