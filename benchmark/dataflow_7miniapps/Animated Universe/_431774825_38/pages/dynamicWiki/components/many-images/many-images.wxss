.many-images {
    padding: 50rpx 0 0 0;
}

.many-images-title {
    margin: 0 24rpx;
}

.many-images-scroll image {
    border-radius: 12rpx;
}

.many-images-scroll {
    margin-top: 40rpx;
    white-space: nowrap;
}

.many-images-type1 {
    width: 220rpx;
    height: 220rpx;
}

.many-images-scroll view {
    display: inline-block;
    margin-left: 16rpx;
}

.many-images-scroll view:first-child {
    margin-left: 24rpx;
}

.many-images-scroll view:last-child {
    margin-right: 24rpx;
}