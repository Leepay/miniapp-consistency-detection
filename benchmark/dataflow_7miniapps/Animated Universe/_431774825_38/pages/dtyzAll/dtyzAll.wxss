.dtyzAll {
    height: 100%;
}

.dtyzAll-main {
    display: flex;
    margin-top: -20rpx;
    min-height: 1032rpx;
}

.dtyzAll-nav-left {
    display: flex;
    flex-direction: column;
    width: 150rpx;
    background: #13131f;
}

.dtyzAll-nav-left view {
    display: flex;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 2rpx solid #262630;
    text-align: center;
}

.dtyzAll-nav-left view image {
    width: 6rpx;
    height: 100rpx;
}

.dtyzAll-nav-left view text {
    width: 100%;
}

.dtyzAll-nav-right {
    padding: 0 22rpx 0 18rpx;
    width: 600rpx;
    background: #1d1f2c;
}

.dtyzAll-nav-right-one {
    margin-top: 44rpx;
}

.dtyzAll-kefu {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 88rpx 0 52rpx;
    font-size: 24rpx;
}

.dtyzAll-kefu image {
    margin-left: 8rpx;
    width: 14rpx;
}

.dtyzAll-active {
    background: #1d1f2c;
}