    align-items: baseline;
}

.card_recharge {
    width: 212rpx;
    text-align: center;
    font-size: var(--font-tg);
    color: #fff;
    background: hsla(0,0%,100%,.19);
    border-radius: 30rpx;
    line-height: 50rpx;
    vertical-align: middle;
    align-self: flex-end;
}

.card_recharge_no_color {
    background: rgba(128,102,59,.06);
    color: #80663b;
}

