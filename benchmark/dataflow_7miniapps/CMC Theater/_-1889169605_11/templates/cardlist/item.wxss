}

.level {
    margin-top: 44rpx;
    margin-bottom: 10rpx;
    font-size: var(--font-tb);
    font-weight: 500;
}

.level,.name {
    width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.name {
    font-size: var(--font-tg);
}

.card_type {
    font-size: var(--font-tf);
    border-radius: 30rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin-top: 10rpx;
    color: #fff;
}

.card_num {
    position: absolute;
    top: 166rpx;
    font-size: var(--font-tc);
}

.integral,.valid_time {
    font-size: var(--font-tg);
}

.valid_time {
    margin-top: 16rpx;
    line-height: 36rpx;
    vertical-align: middle;
}

.unit_list {
    font-size: var(--font-td);
}

.balance {
    font-size: 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-left: 10rpx;
    margin-right: 6rpx;
    font-weight: 500;
    max-width: 220rpx;
}

.integral_bottom_parent {
    display: flex;
    flex-direction: row;
    margin-bottom: 44rpx;
    justify-content: space-between;
}

.integral_bottom {
    display: flex;
    flex-direction: column;
    align-self: flex-end;
}

.chongzhi {
    color: #7e5714;
}

.quanyi {
    color: #424757;
}

.white {
    color: #fff;
}

.card_balance_parent {
    flex-direction: column;
}

.card_balance,.card_balance_parent {
    display: flex;
    justify-content: flex-end;
}

.card_balance {
