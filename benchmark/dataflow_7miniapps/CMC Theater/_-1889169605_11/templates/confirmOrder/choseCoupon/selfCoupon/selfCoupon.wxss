.selfcoupon_item_parent {
    display: flex;
    flex-direction: column;
}

.selfcoupon_nouser_num {
    margin-left: 35rpx;
    margin-top: 12rpx;
    margin-bottom: 16rpx;
    color: var(--color-cd);
    font-size: var(--font-td);
}

.selfcoupon-couponcell {
    width: 680rpx;
    margin-left: 35rpx;
    margin-bottom: 22rpx;
    height: 174rpx;
    display: flex;
    flex-direction: row;
}

.selfcoupon-couponcell-bgimg {
    width: 680rpx;
    height: 174rpx;
    position: absolute;
    z-index: -1;
}

.selfcoupon-cell-startview {
    width: 178rpx;
    text-align: right;
    color: #fff;
}

.selfcoupon-cell-pricetop,.selfcoupon-cell-startview {
    height: 100%;
    display: flex;
    flex-direction: column;
}

.selfcoupon-cell-pricetop,.selfcoupon-startview-priceview {
    justify-content: center;
}

.selfcoupon-startview-priceview {
    width: 100%;
    line-height: 64rpx;
    height: 64rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.selfcoupon-startview-dallor {
    margin-right: 3rpx;
    margin-top: 2rpx;
    line-height: 32rpx;
    font-size: var(--font-th);
}

.selfcoupon-startview-price {
    height: 100%;
    line-height: 64rpx;
    font-size: var(--font-tb);
    font-weight: 500;
}

.selfcoupon-startview-yuan {
    margin-left: 3rpx;
    margin-top: 4rpx;
    line-height: 36rpx;
    font-size: var(--font-tg);
}

.selfcoupon-startview-desc {
    text-align: center;
    width: 100%;
    height: 36rpx;
    font-size: var(--font-tg);
    font-weight: 500;
}

.desc-font-th {
    font-size: var(--font-th);
    font-weight: 400;
}

.selfcoupon-startview-message {
    height: 40rpx;
    line-height: 40rpx;
    width: 100%;
    text-align: center;
    font-size: var(--font-th);
    flex-shrink: 0;
}

.selfcoupon-cell-middleview {
    width: 425rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
}

.selfcoupon-middleview-desc {
    margin-top: 27rpx;
    line-height: 36rpx;
    font-size: var(--font-tg);
    margin-left: 20rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.selfcoupon-middleview-time {
    margin-top: 15rpx;
    height: 72rpx;
    line-height: 72rpx;
    font-size: var(--font-th);
    margin-left: 20rpx;
    color: var(--color-cd);
}

.couponcell-middleview-timeview {
    margin-top: 15rpx;
    height: 72rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.couponcell-middleview-time {
    font-size: var(--font-te);
    margin-left: 20rpx;
    color: var(--color-cd);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.visi-hidden {
    visibility: hidden;
}

.couponcell-middleview-expire-desc {
    margin-left: 12rpx;
    line-height: 26rpx;
    text-align: center;
    font-size: var(--font-th);
    color: var(--color-ce);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.selfcoupon-cell-endview {
    width: 78rpx;
    height: 100%;
}

.selfcoupon-cell-endimage {
    position: absolute;
    width: 78rpx;
    height: 174rpx;
    z-index: -1;
}

.selfcoupon-couponcell-choseiconview {
    width: 100%;
    height: 100%;
    display: flex;
}

.selfcoupon-couponcell-choseicon {
    margin: auto;
    width: 42rpx;
    height: 42rpx;
}

.selfcoupon-couponcell-icon {
    margin: auto;
    background: #fff;
    border-radius: 20rpx;
}

.selfcoupon-selectbuttomview {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 180rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    transition: bottom .3s;
}

.selfcoupon-selectbuttomview-hidden {
    bottom: -180rpx;
}

.selfcoupon-deleateBottomview {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 116rpx;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    transition: bottom .3s;
}

.selfcoupon-deleateBottomview-hidden {
    bottom: -116rpx;
}

.selfcoupon-deleateBtn {
    margin: auto;
    text-align: center;
    width: 700rpx;
    height: 80rpx;
    background-color: var(--color-cb);
    font-size: var(--font-tc);
    line-height: 80rpx;
    border-radius: 8rpx;
    color: #fff;
}

.selfcoupon-selectbuttomview-topview {
    width: 100%;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.selfcoupon-selectbuttomview-pricemessage {
    width: 410rpx;
    height: 80rpx;
    line-height: 30rpx;
    font-size: var(--font-tg);
    color: var(--color-cb);
    margin-left: 25rpx;
    overflow: hidden;
    flex-wrap: wrap;
    white-space: pre-wrap;
    flex-direction: column;
}

.selfcoupon-selectbuttomview-pricemessage,.selfcoupon-selectbuttomview-totalprice {
    display: flex;
    justify-content: center;
}

.selfcoupon-selectbuttomview-totalprice {
    line-height: 80rpx;
    font-size: var(--font-td);
    margin-right: 25rpx;
    flex-direction: row;
    align-items: center;
}

.selfcoupon-selectbuttomview-totalprice-price {
    color: var(--color-ce);
    margin-right: 5rpx;
    font-size: var(--font-tb);
}

.selfcoupon-selectbuttomview-btn {
    margin-top: 5rpx;
    width: 100%;
    height: 75rpx;
    display: flex;
    flex-direction: row;
    text-align: center;
    font-size: var(--font-tc);
}

.selfcoupon-selectbuttomview-usecouponbtn {
    margin-left: 25rpx;
    width: 700rpx;
    height: 75rpx;
    background-color: var(--color-cb);
    line-height: 75rpx;
    border-radius: 8rpx;
    color: #fff;
}

.couponcell-middleview-new {
    margin-left: 8rpx;
    width: 26rpx;
    height: 26rpx;
    line-height: 26rpx;
    text-align: center;
    background-color: var(--color-ce);
    font-size: var(--font-tf);
    color: #fff;
    border-radius: 4rpx;
}

.couponcell_package_name_parent {
    display: flex;
}

.couponcell_package_name {
    font-size: var(--font-te);
    color: var(--color-cd);
    margin-left: 20rpx;
    text-align: left;
    padding: 0 4rpx;
    border: 2rpx solid hsla(0,0%,77.3%,.5);
    border-radius: 6rpx;
}

.middelView-nousecolor {
    color: var(--color-ci);
}