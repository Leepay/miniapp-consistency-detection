.online_coupon_cell_parent {
    flex-direction: column;
    margin-left: 35rpx;
    width: 680rpx;
    position: relative;
    padding-top: 10rpx;
}

.off_line_coupon_nouser_parent,.online_coupon_cell_parent {
    display: flex;
}

.off_line_coupon_nouser_num {
    margin-top: 12rpx;
    margin-bottom: 16rpx;
    color: var(--color-cd);
    font-size: var(--font-td);
}

.online_coupon_cell {
    width: 680rpx;
    height: 174rpx;
    display: flex;
    flex-direction: row;
    position: relative;
}

.online_couopn_bgimg {
    width: 680rpx;
    height: 174rpx;
    position: absolute;
    z-index: 0;
}

.online_coupon_start {
    width: 178rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    z-index: 1;
}

.online_coupon_start_movie {
    color: #ff5600;
    font-size: var(--font-tc);
    margin-top: 54rpx;
    font-weight: 500;
}

.online_coupon_mine_start_text {
    color: #ff5600;
    font-size: var(--font-tf);
    margin-top: 6rpx;
    line-height: 28rpx;
}

.coupon_business_price {
    height: 100%;
    line-height: 64rpx;
    font-size: 54rpx;
    font-weight: 500;
    color: #32bb99;
}

.coupon_business_yuan {
    margin-left: 3rpx;
    margin-top: 4rpx;
}

.coupon_business_text,.coupon_business_yuan {
    color: #32bb99;
    line-height: 36rpx;
    font-size: var(--font-tg);
    font-weight: 500;
}

.online_coupon_exchange_rule {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: 30rpx;
    height: 32rpx;
}

.online_coupon_exchange_title {
    color: #ff5600;
    font-size: var(--font-th);
}

.online_coupon_exchange_image {
    width: 24rpx;
    height: 24rpx;
    margin-left: 8rpx;
}

.online_coupon_middleview {
    height: 100%;
    display: flex;
    flex-direction: column;
    z-index: 1;
    flex: 1;
}

.online_coupon_title {
    height: 36rpx;
    margin-top: 27rpx;
    line-height: 36rpx;
    font-size: var(--font-tg);
    color: var(--color-cc);
    margin-left: 23rpx;
    white-space: normal;
    overflow: hidden;
    text-overflow: ellipsis;
}

.online_coupon_num_prent {
    display: flex;
}

.online_coupon_num {
    font-size: var(--font-th);
    color: var(--color-cd);
    margin-left: 23rpx;
    margin-top: 8rpx;
    text-align: left;
    padding: 0 4rpx;
    border: 2rpx solid hsla(0,0%,77.3%,.5);
    border-radius: 6rpx;
}

.online_coupon_timeview {
    display: flex;
    margin-left: 23rpx;
    flex: 1;
    align-items: center;
}

.online_coupon_time {
    font-size: var(--font-te);
    color: var(--color-cd);
}

.online_coupon_new {
    margin-left: 4rpx;
    padding: 2rpx 4rpx;
    text-align: center;
    background-color: var(--color-ce);
    font-size: var(--font-tf);
    color: #fff;
    border-radius: 4rpx;
}

.visi-hidden {
    visibility: hidden;
}

.online_coupon_expire_desc {
    margin-left: 8rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-size: var(--font-th);
    color: #fe4632;
}

.middelView-nousecolor {
    color: var(--color-ci);
}

.online_coupon_endview {
    width: 78rpx;
    height: 100%;
    z-index: 1;
    position: relative;
}

.online_coupon_endimage {
    position: absolute;
    width: 78rpx;
    height: 174rpx;
    z-index: -1;
}

.online_coupon_choseiconview {
    width: 100%;
    height: 100%;
    display: flex;
    position: relative;
}

.online_couponcell-icon {
    position: absolute;
    left: -20rpx;
    top: 0rpx;
    width: 95rpx;
    height: 95rpx;
}

.online_coupon_detail_icon {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    right: 0rpx;
    bottom: 0rpx;
}

.online_coupon_share_top {
    padding-left: 10rpx;
    padding-right: 10rpx;
    background: linear-gradient(270deg,#ff374a 0,#f909d0);
    border-radius: 0 12rpx 0 12rpx;
    height: 40rpx;
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    z-index: 2;
    line-height: 40rpx;
    text-align: center;
    vertical-align: middle;
    color: #fff;
    font-size: var(--font-te);
}

.online_coupon_choseicon {
    margin: auto;
    width: 42rpx;
    height: 42rpx;
}

.online_coupon_icon {
    margin: auto;
    background: #fff;
    border-radius: 20rpx;
}

.online_coupon_desc_bottom {
    display: flex;
    flex-direction: column;
    width: 680rpx;
    background-color: #fafafa;
}

.online_coupon_desc_info {
    width: 680rpx;
    color: var(--color-cd);
    font-size: var(--font-te);
    line-height: 36rpx;
    padding: 16rpx 10rpx 16rpx 24rpx;
    box-sizing: border-box;
    white-space: pre-line;
}

.online_coupon_num_bottom_parent {
    display: flex;
    flex-direction: column;
}

.online_coupon_num_bottom_line {
    height: 1rpx;
    margin-left: 35rpx;
    margin-right: 35rpx;
    border: none;
    border-bottom: 1px dashed #c5c5c5;
    transform: scaleY(.5);
    transform-origin: 0 0;
}

.online_coupon_num_bottom {
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 80rpx;
    margin-left: 35rpx;
    margin-right: 35rpx;
}

.online_coupon_num_bottom_text {
    color: var(--color-cd);
    font-size: var(--font-te);
}

.online_coupon_num_bottom_value {
    flex: 1;
    color: var(--color-cd);
    font-size: var(--font-tb);
    text-align: right;
}

.coupon_shixiao_white {
    color: #fff;
}

.coupon_shixiao_c5 {
    color: #c5c5c5;
}