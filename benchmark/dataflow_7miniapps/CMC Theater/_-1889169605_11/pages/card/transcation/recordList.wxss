page {
    background-color: #fff;
}

.group {
    height: 50rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-left: 32rpx;
    padding-right: 32rpx;
}

.child {
    padding-left: 32rpx;
}

.child,.child_item_parent {
    height: 140rpx;
    display: flex;
    flex-direction: column;
}

.child_item_parent {
    justify-content: center;
    padding-right: 32rpx;
}

.child_item {
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.tran_time {
    font-size: var(--font-te);
    color: var(--color-cd);
}

.tran_type {
    font-size: var(--font-td);
    color: var(--color-cc);
}

.tran_status {
    font-size: var(--font-te);
    color: var(--color-cd);
}

.resumeRecordOriange {
    color: var(--color-cb);
}

.resumeRecordBlack {
    color: #000;
}