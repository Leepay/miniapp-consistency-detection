.pwd_parent,.pwd_screen {
    width: 100%;
    height: 100%;
}

.pwd_screen {
    position: fixed;
    z-index: 1000;
    top: 0;
    left: 0;
    background-color: #000;
    opacity: 0;
    overflow: hidden;
    color: #fff;
    transition: background-color .3s;
}

.pwd_screen_anim {
    opacity: .2;
}

.pwd_body {
    width: 100%;
    position: fixed;
    z-index: 1000;
    height: 0rpx;
    bottom: 0rpx;
    transition: height .3s;
    background: #fff;
}

.pwd_body_anim {
    height: 790rpx;
}

.pwd_dialog_close {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    vertical-align: middle;
    background: #fff;
}

.text_close {
    width: 100%;
    position: absolute;
    margin: 0 auto;
    text-align: center;
}

.image_close {
    width: 32rpx;
    height: 30rpx;
    position: absolute;
    right: 32rpx;
    top: 36rpx;
}

.pwd_input {
