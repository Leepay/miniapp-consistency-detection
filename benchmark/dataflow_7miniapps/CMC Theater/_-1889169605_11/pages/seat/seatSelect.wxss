page {
    width: 100%;
    height: 100%;
    background-color: var(--color-cg);
}

.hall_reminder {
    width: 100%;
    padding: 10rpx 20rpx 10rpx 32rpx;
    box-sizing: border-box;
    vertical-align: middle;
    background: #fff7e0;
    color: var(--color-cb);
    font-size: var(--font-te);
}

.body {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
}

.test_sv {
    height: 340px;
    position: relative;
}

.info_parent {
    flex-direction: column;
    background: #fff;
}

.info,.info_parent {
    display: flex;
}

.info {
    flex-direction: row;
    justify-content: space-between;
    align-items: baseline;
    height: 34rpx;
    padding-left: 35rpx;
    padding-right: 35rpx;
}

.name {
    font-size: var(--font-tc);
    color: var(--color-cc);
    margin-left: 35rpx;
    margin-right: 30rpx;
    height: 40rpx;
    margin-top: 20rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.time {
    font-size: var(--font-te);
    color: var(--color-cb);
}

.format {
    flex: 1;
    margin-left: 10rpx;
}

.change,.format {
    font-size: var(--font-te);
    color: var(--color-cc);
}

.jiantou_parent {
    display: flex;
    flex-direction: column;
    height: 34rpx;
    margin-top: 20rpx;
    background: var(--color-cg);
}

.seat_jiantou {
    width: 94rpx;
    height: 28rpx;
    position: relative;
    top: -3rpx;
    right: 35rpx;
    align-self: flex-end;
}

.time_list {
    display: flex;
    flex-direction: row;
}

.sv_time_list {
    white-space: nowrap;
    overflow: scroll;
    padding-left: 16rpx;
    box-sizing: border-box;
}

.sv_time_list_item {
    height: 130rpx;
    padding-top: 10rpx;
    flex-direction: row;
}

.sv_time_list_item,.time_info {
    display: flex;
}

.time_info {
    flex-direction: column;
    width: 126rpx;
    height: 126rpx;
    margin-right: 16rpx;
    border-radius: 4rpx;
    align-items: center;
    justify-content: center;
    padding-left: 10rpx;
    padding-right: 10rpx;
}

.v_line_time_head {
    width: 40rpx;
    display: block;
    background: var(--color-cg);
    height: 130rpx;
    text-align: center;
    writing-mode: vertical-lr;
    margin: auto 16rpx auto 0;
}

.time_head {
    font-size: var(--font-te);
    color: var(--color-cd);
}

.start_time {
    font-size: var(--font-td);
    color: var(--color-cc);
}

.format_list {
    color: var(--color-cd);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.format_list,.price {
    font-size: var(--font-te);
}

.price {
    color: var(--color-ce);
}

.seat_status_parent {
    justify-content: space-between;
    padding-left: 30rpx;
    padding-right: 30rpx;
    margin-bottom: 10rpx;
    background-color: var(--color-cg);
    width: 100%;
    box-sizing: border-box;
}

.seat_status,.seat_status_parent {
    display: flex;
    flex-direction: row;
    height: 32rpx;
}

.seat_status {
    align-items: center;
}

.image_top {
    width: 36rpx;
    height: 32rpx;
}

.image {
    width: 36rpx;
    height: 100%;
}

.image_lv {
    width: 80rpx;
    height: 32rpx;
}

.seat_status_name {
    font-size: var(--font-te);
    color: var(--color-cd);
    margin-left: 8rpx;
}

.area_parent {
    display: flex;
    height: 36rpx;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-left: 30rpx;
    padding-right: 30rpx;
    width: 100%;
    box-sizing: border-box;
}

.area_image {
    width: 24rpx;
    height: 24rpx;
}

.area_cell {
    flex-direction: row;
    align-items: baseline;
}

.area_cell,.bottom_parent {
    display: flex;
}

.bottom_parent {
    width: 100%;
    position: fixed;
    background: #fff;
    bottom: 0rpx;
    z-index: 2;
    flex-direction: column;
}

.recommend {
    position: absolute;
    top: 0;
    font-size: var(--font-te);
    color: var(--color-cc);
    padding-left: 25rpx;
    line-height: 80rpx;
    height: 80rpx;
    width: auto;
    vertical-align: middle;
}

.recommend_seats {
    display: flex;
    margin-top: 80rpx;
    flex-direction: column;
    justify-content: flex-start;
}

.sv_can_buy_seats {
    display: flex;
    white-space: nowrap;
    overflow: scroll;
}

.sv_can_buy_seats_item {
    display: inline-flex;
    height: 80rpx;
    justify-content: space-between;
}

.recommend_number {
    font-size: var(--font-te);
    color: var(--color-cc);
    line-height: 80rpx;
    vertical-align: middle;
    flex: 1;
    text-align: center;
}

.v_line {
    width: 1px;
    background: var(--color-ch);
    height: 100%;
}

.selected_seat_parent {
    display: flex;
    flex-direction: column;
}

.selected_seat {
    padding-left: 37rpx;
    padding-right: 37rpx;
    width: 100%;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.selected_seat,.selected_seat_child {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.selected_seat_child {
    width: 156rpx;
    height: 68rpx;
    margin-top: 10rpx;
    margin-right: 8rpx;
    margin-left: 10rpx;
    justify-content: center;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

.selected_seat_delete {
    width: 14rpx;
    height: 14rpx;
    margin-left: 10rpx;
}

.selected_seat_name {
    font-size: var(--font-te);
    color: var(--color-cc);
}

.selected_seat_price_info {
    font-size: var(--font-te);
    color: var(--color-cd);
}

.selected_price_parent {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.seat_all_price_parent {
    padding-left: 32rpx;
    padding-right: 32rpx;
    padding-top: 20rpx;
    height: 60rpx;
    flex-direction: row;
    justify-content: space-between;
}

.seat_all_price_details_parent,.seat_all_price_parent {
    display: flex;
    -webkit-flex-direction: row;
    align-items: baseline;
}

.seat_all_price_details_parent {
    flex-direction: row;
    padding-left: 10rpx;
    padding-right: 10rpx;
    flex: 1;
}

.selected_seat_clear {
    font-size: var(--font-te);
    color: var(--color-cb);
}

.selected_seat_price {
    font-size: var(--font-tb);
    color: var(--color-ce);
}

.wrap {
    flex: 1;
    background-color: var(--color-cg);
    position: relative;
    overflow: hidden;
}

.room {
    margin: 0 auto;
    text-align: center;
    font-size: var(--font-te);
    font-weight: 500;
    width: 100%;
    height: 90rpx;
    background: var(--color-cg);
    display: flex;
    flex-direction: column;
    align-items: center;
}

.roomL {
    vertical-align: middle;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    margin-top: 10rpx;
    white-space: nowrap;
    margin-bottom: 10rpx;
    min-width: 420rpx;
    color: var(--color-cd);
    height: 32rpx;
    line-height: 32rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-image: url("data:image/png;base64,");
}

.roomL,.roomL2 {
    letter-spacing: 2rpx;
}

.roomL2 {
    font-size: var(--font-tf);
    color: var(--color-cf);
    padding-left: 10rpx;
    padding-right: 10rpx;
    height: 28rpx;
    background: var(--color-ci);
    border-radius: 6rpx;
}

.sv_parent {
    position: relative;
    background: var(--color-cg);
}

.map_parent {
    background: var(--color-cg);
}

.map {
    position: absolute;
    margin-left: 72rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.willChange {
    will-change: transform;
}

.row {
    display: flex;
}

.columnN,.columnN_parent {
    position: absolute;
    z-index: 2;
    top: 100rpx;
}

.columnN {
    margin-left: 24rpx;
    width: 36rpx;
    background-color: #c2c2c2;
    opacity: .8;
    border-radius: 12rpx;
}

.columnNum {
    height: var(--seat-height);
    line-height: var(--seat-height);
    color: #fff;
    text-align: center;
    margin-top: 10rpx;
    font-size: 28rpx;
    font-family: Geneva,Tahoma,sans-serif;
}

.centerTips {
    font-size: 22rpx;
    width: 100rpx;
    height: 34rpx;
    background-color: #e0e0e0;
    text-align: center;
    line-height: 34rpx;
    border-radius: 6rpx;
}

.seat_img_parent {
    height: var(--seat-height);
    margin-top: 10rpx;
    margin-right: 10rpx;
}

.empty,.seat-img {
    width: var(--seat-width);
    height: 100%;
}

.seat_img_ql_parent {
    height: var(--seat-height);
    margin-top: 10rpx;
}

.seat_img_ql,.seat_img_ql_r {
    width: var(--seat-widthql);
    height: 100%;
}

.seat_img_ql_r {
    margin-right: 10rpx;
}

.time_select_color {
    color: var(--color-cb);
}

.seat_sure_parent {
    width: 100%;
    position: absolute;
    display: flex;
    bottom: 0;
    height: 112rpx;
    flex-direction: column;
    background: #fff;
    font-weight: 500;
}

.seat_sure {
    height: 74rpx;
    margin: 20rpx;
    color: #fff;
    vertical-align: middle;
    text-align: center;
    line-height: 74rpx;
    background-color: var(--color-cb);
    font-size: var(--font-td);
    border-radius: 6rpx;
}

.is-hidden {
    display: none;
}