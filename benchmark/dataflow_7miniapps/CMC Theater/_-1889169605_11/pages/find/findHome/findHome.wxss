.findshop-view {
    width: 100%;
    height: 420rpx;
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
}

.findshop-view-cell {
    width: 50%;
    height: 100%;
}

.findshop-view-cell-little {
    width: 50%;
    height: 50%;
}

.findshop-view-cellView {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
}

.findshop-view-cellImageView {
    width: 100%;
    height: 292rpx;
    display: flex;
}

.findshop-view-cellImage {
    margin: auto;
    width: 258rpx;
    height: 258rpx;
    background-size: 100%;
    background-image: var(--shop-defimage);
}

.findshop-view-huiview {
    max-width: 70rpx;
    height: 30rpx;
    margin-top: 4rpx;
    margin-left: 24rpx;
    position: absolute;
}

.findshop-view-celldes {
    margin-top: -3rpx;
    display: flex;
    font-size: var(--font-td);
    margin-left: 24rpx;
    margin-right: 24rpx;
    text-indent: 60rpx;
    width: 326rpx;
    height: 100%;
}

.findshop-view-celldes-little {
    width: 140rpx;
    line-height: 45rpx;
    word-break: break-all;
}

.findshop-view-desview {
    overflow: hidden;
    height: 70rpx;
}

.findshop-view-price {
    font-size: var(--font-tc);
    font-weight: 500;
    margin-left: 24rpx;
    height: 44rpx;
    line-height: 44rpx;
    color: var(--color-ce);
    display: flex;
}

.findshop-view-count {
    margin-left: 24rpx;
    font-size: var(--font-tf);
    color: var(--color-ce);
}

.findshop-view-cellImageView-little {
    width: 214rpx;
    height: 100%;
    display: flex;
}

.findshop-view-cellImage-little {
    margin: auto auto auto 0rpx;
    width: 170rpx;
    height: 170rpx;
    background-size: 100%;
    background-image: var(--shop-defimage);
}

.findshop-view-desview-little {
    margin-top: 23rpx;
    height: 130rpx;
}

.findshop-view-price-little {
    margin-left: 24rpx;
    width: 140rpx;
    height: 44rpx;
    line-height: 44rpx;
}

.is-hidden {
    display: none;
}

.lineview {
    width: 100%;
    height: 1rpx;
    background-color: var(--color-ch);
}

.middlelineone {
    width: 1rpx;
    height: 420rpx;
}

.middlelineone,.middlelinetwo {
    background-color: var(--color-ch);
    left: 375rpx;
    position: absolute;
}

.middlelinetwo {
    width: 375rpx;
    height: 2rpx;
    margin-top: 210rpx;
}

.findfood-view {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.findfood-view-cell {
    width: 220rpx;
    height: 100%;
    margin-left: 22.5rpx;
}

.findfood-cell-imageview {
    width: 100%;
    height: 220rpx;
    background-image: var(--shop-defimage);
    background-size: 100%;
}

.findfood-cell-image {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}

.findfood-cell-nameview {
    margin-top: 16rpx;
    height: 40rpx;
    overflow: hidden;
    font-size: var(--font-tg);
    display: flex;
    flex-direction: row;
}

.findfood-cell-huiview {
    flex-shrink: 0;
    margin-top: 5rpx;
    min-width: 50rpx;
    max-width: 70rpx;
    height: 30rpx;
}

.findfood-cell-name {
    margin-left: 4rpx;
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.findfood-cell-price {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    color: var(--color-ce);
    font-size: var(--font-tc);
    font-weight: 500;
    margin-top: 4rpx;
    display: flex;
}

.findfood-cell-count {
    margin-left: 24rpx;
    font-size: var(--font-tf);
    color: var(--color-ce);
}

.confirmGoodcell_add_peice_text {
    font-size: var(--font-tf);
    font-weight: 400;
    color: #fe4632;
    margin-left: 4rpx;
    margin-top: 2rpx;
}

.findfood-cell-addbtn {
    width: 75rpx;
    margin-left: 145rpx;
}

.findfood-cell-addbtn,.findfood-cell-optionaddbtn {
    height: 75rpx;
    margin-top: 145rpx;
    display: flex;
    position: absolute;
}

.findfood-cell-optionaddbtn {
    width: 150rpx;
    margin-left: 70rpx;
}

.findfood-cell-addbtnimg {
    width: 48rpx;
    height: 48rpx;
    margin: auto;
}

.is-hidden {
    display: none;
}

page {
    background-color: #fff;
}

.advert-swiper {
    width: 100%;
    height: 200rpx;
}

.advert-swiper-item {
    width: 100%;
    height: 100%;
}

.find-first-group {
    width: 100%;
}

.find-shop-group {
    width: 100%;
    height: 422rpx;
}

.find-food-group {
    width: 100%;
    height: 310rpx;
}

.find-third-group {
    width: 100%;
}

.find-promcell {
    width: 100%;
    display: flex;
    flex-direction: column;
    margin-bottom: 24rpx;
}

.find-promcell-image {
    width: 710rpx;
    height: 280rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    background-size: 100%;
    background-image: url(data:image/png;base64,);
}

.find-promcell-statusicon {
    margin-left: 616rpx;
    margin-top: 10rpx;
    position: absolute;
    width: 115rpx;
    height: 35rpx;
}

.find-promcell-desc {
    overflow: hidden;
    text-overflow: ellipsis;
    display: flex;
    flex-direction: column;
    justify-content: center;
    height: 80rpx;
    line-height: 40rpx;
    font-size: var(--font-td);
}

.find-promcell-desc,.find-promcell-line {
    width: 710rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}

.find-promcell-line {
    height: 2rpx;
    background-color: var(--color-ch);
}

.find-title {
    width: 100%;
    height: 92rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.find-title-text {
    font-size: var(--font-tb);
    font-weight: 500;
    text-align: center;
}

.find-title-line {
    margin: 30rpx;
    height: 2rpx;
    width: 44rpx;
    background-color: var(--color-cd);
}

.find-chose-more {
    width: 100%;
    height: 72rpx;
    display: flex;
}

.find-more-btn {
    margin: auto;
    width: 152rpx;
    height: 48rpx;
    line-height: 48rpx;
    background-color: var(--color-cb);
    color: #fff;
    font-size: var(--font-te);
    text-align: center;
    border-radius: 24rpx;
}

.find-gray-view {
    width: 100%;
    height: 20rpx;
    background-color: var(--color-cg);
}

.is-hidden {
    display: none;
}