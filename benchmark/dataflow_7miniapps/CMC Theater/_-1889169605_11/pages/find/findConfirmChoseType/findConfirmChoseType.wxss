.supper-picker {
    display: flex;
    flex-direction: row;
}

.findPicker-view {
    height: 430rpx;
    transition: width .3s;
    background-color: var(--color-cf);
}

.width0 {
    width: 0;
}

.width20 {
    width: 20%;
}

.width25 {
    width: 25%;
}

.width40 {
    width: 40%;
}

.width50 {
    width: 50%;
}

.width60 {
    width: 60%;
}

.width100 {
    width: 100%;
}

.findPicker-column {
    line-height: 60rpx;
    text-align: center;
    height: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.color-gray {
    color: var(--color-cd);
}

.align-left {
    text-align: left;
}

.align-right {
    text-align: right;
}

page {
    overflow: hidden;
    background-color: var(--color-cg);
}

.findconfirmchosetype-moderightimage {
    width: 34rpx;
    height: 24rpx;
    margin-right: 26rpx;
}

.picker-view {
    height: 430rpx;
    width: 100%;
    transition: height .3s;
    overflow: hidden;
    background-color: var(--color-cf);
}

.findconfirmchosetype-bottomview {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 125rpx;
    display: flex;
}

.findconfirmchosetype-bottomview-btn {
    text-align: center;
    width: 710rpx;
    height: 75rpx;
    line-height: 75rpx;
    margin: auto auto auto 20rpx;
    background-color: var(--color-cb);
    font-size: var(--font-td);
    font-weight: 500;
    color: #fff;
    border-radius: 4rpx;
}

.findconfirmchosetype-modeview {
    overflow: hidden;
    width: 100%;
    height: 105rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    transition: height .3s;
}

.findconfirmchosetype-modeleftview {
    height: 100%;
    line-height: 105rpx;
    width: 300rpx;
    margin-left: 20rpx;
    font-size: var(--font-td);
}

.findconfirmchosetype-moderightview {
    line-height: 60rpx;
    margin-right: 26rpx;
    font-size: var(--font-td);
    display: flex;
    flex-direction: row;
    align-items: center;
    text-align: right;
}

.arrow-view {
    margin-top: -3rpx;
    margin-left: 8rpx;
    width: 14rpx;
    height: 25rpx;
}

.findconfirmchosetype-gray {
    width: 100%;
    height: 16rpx;
    background-color: var(--color-cg);
}

.findconfirmchosetype-lineview {
    width: 730rpx;
    margin-left: 20rpx;
    margin-bottom: 1rpx;
    height: 1rpx;
    background-color: var(--color-ch);
}

.longline {
    width: 100%;
    margin-left: 0;
}

.chosetype-picrkeHeightView {
    height: 430rpx;
    overflow: hidden;
    transition: height .3s;
}

.chosetype-modeHeightView {
    height: 105rpx;
    overflow: hidden;
    transition: height .3s;
}

.color-gray {
    color: var(--color-cd);
}

.viewheightzero {
    height: 0;
}

.is-hidden {
    display: none;
}