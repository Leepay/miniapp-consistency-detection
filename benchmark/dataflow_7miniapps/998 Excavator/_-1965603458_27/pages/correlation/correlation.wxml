<view class="page">
    <view class="text">工程圈
        <image src="./../../images/icon/hehe.png"></image>
    </view>
    <view class="content">
        <view class="list" wx:for="{{items}}">
            <view class="logo_png">
                <image src="{{item.i_png}}"></image>
            </view>
            <view>
                <view class="logo_name">{{item.app_name}}</view>
                <view class="options">
                    <view bindtap="app_skip" id="{{item.id}}" style="border:1px solid {{item.color}}">{{item.used}}</view>
                    <view bindtap="rent_skip" id="{{item.id}}" style="border:1px solid {{item.color}}">{{item.rent}} </view>
                    <view bindtap="talents_skip" id="{{item.id}}" style="border:1px solid {{item.color}}">{{item.talents}}</view>
                </view>
            </view>
        </view>
    </view>
</view>
